				<ice:panelPopup draggable="false" modal="true"
					style="height: 600px; width:830px; color: white; background-color: #0085BC;"
					visible="#{mBeanEntrantesMesa.verPDF}" autoCentre="true"
					styleClass="icePnlPop">
					<f:facet name="header">
						<ice:panelGroup styleClass="largeFacet">
							<ice:outputText value="Visualizador de PDF"
								style="font-size: 12px; color: blue;"
								styleClass="popupHeaderText" />
						</ice:panelGroup>
					</f:facet>
					<f:facet name="body">
						<ice:panelGroup styleClass="largeFacet">
							<ice:panelGrid columns="2">
								<ice:panelTab>
									<ice:panelGroup>
										<!-- file upload usage with progress callback. -->
										<ice:panelGrid>
											<iframe
												src="pdfjs/web/viewer.html?file=#{mBeanEntrantesMesa.rutaPDFPopup}"
												type="application/pdf" width="800px" height="500px" />
										</ice:panelGrid>
									</ice:panelGroup>
								</ice:panelTab>
							</ice:panelGrid>
							<ice:commandButton type="submit" value="Cancelar"
								image="/images/cerrar.png"
								style="size: width: 35px; height: 35px; margin: 0 auto; display: block;"
								actionListener="#{mBeanEntrantesMesa.cancelarDocs}" />
						</ice:panelGroup>
					</f:facet>
				</ice:panelPopup>




				<!-- Alerta de mensaje-->
				<ice:panelPopup draggable="false" modal="true"
					style="width: 270px; height: 160px; background-color: #0085BC;"
					visible="#{mBeanEntrantesMesa.ver}" autoCentre="true"
					styleClass="icePnlPop">
					<f:facet name="header">
						<ice:panelGroup styleClass="popupHeaderWrapper">
							<ice:outputText value="Mensaje" styleClass="popupHeaderText"
								style="font-size: 12px; color: blue;" />
						</ice:panelGroup>
					</f:facet>
					<f:facet name="body">
						<ice:panelGrid columns="1" style="text-align: center;">
							<ice:outputText value="#{mBeanEntrantesMesa.error}"
								style="font-size: 12px; color: white;" />
							<br />
							<ice:commandButton type="submit" value="Aceptar"
								image="/images/cerrar.png"
								style="size: width: 35px; height: 35px"
								actionListener="#{mBeanEntrantesMesa.cerrar}" />
						</ice:panelGrid>
					</f:facet>
				</ice:panelPopup>



				<!-- Popup para cargar un archivo adjunto" />-->
				<ice:panelPopup draggable="false" modal="true"
					style="width: 550px; height: 350px; color: white; background-color: #0085BC;"
					visible="#{mBeanEntrantesMesa.verDetalles}" autoCentre="true"
					styleClass="icePnlPop">
					<f:facet name="header">
						<ice:panelGroup styleClass="popupHeaderWrapper">
							<ice:outputText value="Archivo Adjunto"
								style="font-size: 12px; color: blue;"
								styleClass="popupHeaderText" />
						</ice:panelGroup>
					</f:facet>
					<f:facet name="body">
						<ice:panelGroup styleClass="popupBody">
							<ice:panelGrid columns="2">
								<ice:panelTab label="Carga de Datos" title="Carga de Datos">
									<ice:panelGroup>
										<!-- file upload usage with progress callback. -->
										<ice:panelGrid>
											<ice:outputText value="Seleccione un archivo a cargar:"
												style="font-size: 12px; color: white;" />
											<ice:inputFile id="inputFileName3" uniqueFolder="false"
												uploadDirectoryAbsolute="true" autoUpload="true"
												uploadDirectory="#{msg['path.mesa']}"
												progressListener="#{mCarga.fileUploadProgress}"
												actionListener="#{mCarga.uploadFile}" />
											<!-- progress bar, percent value is upated via progressListener-->

											<ice:outputProgress value="#{mCarga.fileProgress}"
												styleClass="uploadProgressBar" />
											<ice:message for="inputFileName" />
											<br />
											<ice:outputText value="#{mCarga.componentStatus}"
												style="font-size: 12px; color: white;" />

										</ice:panelGrid>


									</ice:panelGroup>
								</ice:panelTab>

							</ice:panelGrid>
							<ice:commandButton id="modalPnlCBError" type="submit"
								value="Salir" image="/images/cerrar.png"
								style="size: width: 35px; height: 35px"
								actionListener="#{mBeanEntrantesMesa.cerrarDetalles}" />
						</ice:panelGroup>
					</f:facet>
				</ice:panelPopup>

				<!-- Popup para la carga -->
				<ice:panelPopup id="modalPnlPopCarga" draggable="false" modal="true"
					style="width: 200px; height: 120px; background-color: #0085BC;"
					visible="#{mCarga.verConfirmacion}" autoCentre="true"
					styleClass="icePnlPop">
					<f:facet name="header">
						<ice:panelGroup styleClass="popupHeaderWrapper">
							<ice:outputText value="Alerta" styleClass="popupHeaderText" />
						</ice:panelGroup>
					</f:facet>
					<f:facet name="body">
						<ice:panelGroup styleClass="popupBody">
							<ice:outputText value="#{mCarga.msg}"
								style="font-size: 12px; color: blue;" />
							<br></br>
							<ice:commandButton type="submit" value="Aceptar" title="Aceptar"
								image="/images/actualizar.png"
								style="size: width: 25px; height: 20px"
								actionListener="#{mCarga.okConfirm}" />
							<ice:outputText value="   " />
							<ice:commandButton value="Cancelar" title="Cancelar"
								image="/images/eliminar.png"
								style="size: width: 25px; height: 20px"
								actionListener="#{mCarga.cancelar}" />
						</ice:panelGroup>
					</f:facet>
				</ice:panelPopup>

				<!-- Alerta de mensaje con confirmacion!!!  -->

				<ice:panelPopup draggable="false" modal="false"
					style="width: 220px; height: 130px; background-color: #0085BC;"
					visible="#{mBeanEntrantesMesa.verAlertaConfirmacionBorrar}"
					autoCentre="true" styleClass="icePnlPop">
					<f:facet name="header">
						<ice:panelGroup styleClass="popupHeaderWrapper">
							<ice:outputText value="Mensaje" styleClass="popupHeaderText"
								style="font-size: 12px; color: blue;" />
						</ice:panelGroup>
					</f:facet>
					<f:facet name="body">
						<ice:panelGroup styleClass="popupBody">
							<center> <ice:outputText 
								value="#{mBeanEntrantesMesa.msg}" 
								style="font-size: 12px; color: white;" /> <br></br>
							<br></br>
							<ice:commandButton type="submit" value="    SI    "
								actionListener="#{mBeanEntrantesMesa.confirmaBorrado}" /> 
								<ice:outputText value="    " />
								<ice:commandButton
								value="    NO   "
								actionListener="#{mBeanEntrantesMesa.cancelaBorrado}" /> </center>
						</ice:panelGroup>
					</f:facet>
				</ice:panelPopup>

				<!-- Popup Asuntos Estadares -->
				<ice:panelPopup id="modalPndoc" draggable="false" modal="true"
					style="width: 600px; height: 400px; color: white; background-color: #0085BC;"
					visible="#{mBeanEntrantesMesa.verdocs}" autoCentre="true"
					styleClass="icePnlPop">
					<f:facet name="header">
						<ice:panelGroup styleClass="popupHeaderWrapper">
							<ice:outputText value="Asuntos Estándares" style="color:blue;"
								styleClass="popupHeaderText" />
						</ice:panelGroup>
					</f:facet>
					<f:facet name="body">
						<ice:panelGroup styleClass="popupBody">
							<ice:panelGrid columns="1">
								<ice:panelGrid columns="7" cellpadding="1" border="1">
									<h:selectOneRadio value="#{mBeanEntrantesMesa.opcion_asunto}"
										style="font-size: 9px; color:white;">
										<f:selectItem itemValue="ASUNTO" itemLabel="ASUNTO" />
									</h:selectOneRadio>
									<ice:inputText value="#{mBeanEntrantesMesa.detalle_asunto}"
										style="font-size: 10px;" size="50"
										onchange="this.value = this.value.toUpperCase();" />
									<ice:commandButton value="Ejecutar"
										actionListener="#{mBeanEntrantesMesa.buscarAsunto}"
										title="Buscar" image="/images/buscar2.png"
										style="size: width: 25px; height: 25px">
									</ice:commandButton>
									<ice:commandButton value="Refrescar"
										actionListener="#{mBeanEntrantesMesa.RefrescarAsunto}"
										title="Refrescar" image="/images/resert.png"
										style="size: width: 25px; height: 25px">
									</ice:commandButton>
								</ice:panelGrid>

								<ice:dataTable var="myDocs"
									value="#{mBeanEntrantesMesa.itemsAsuntos}" scrollable="true"
									scrollHeight="240px" columnWidths="30px,500px;">
									<ice:column>
										<ice:rowSelector value="#{myDocs.selected}" />
										<f:facet name="header">
											<ice:outputText value="Item" />
										</f:facet>
										<ice:outputText value="#{myDocs.codigo}"
											style="font-size: 9px;" />
									</ice:column>

									<ice:column>
										<f:facet name="header">
											<ice:outputText value="Asunto" />
										</f:facet>
										<ice:outputText value="#{myDocs.descripcion}"
											style="font-size: 9px;" />
									</ice:column>

								</ice:dataTable>
							</ice:panelGrid>
							<ice:panelGrid columns="2">
								<ice:commandButton type="submit" value="Aceptar"
									image="/images/boton_aceptar.png"
									style="size: width: 35px; height: 35px"
									actionListener="#{mBeanEntrantesMesa.cerrarDocs}" />
								<ice:commandButton type="submit" value="Cancelar"
									image="/images/boton_cancelar.jpg"
									style="size: width: 35px; height: 35px"
									actionListener="#{mBeanEntrantesMesa.cancelarDocs}" />
							</ice:panelGrid>
						</ice:panelGroup>
					</f:facet>
				</ice:panelPopup>

			</ice:panelTab>
			<!--
		<ice:panelTab label="Seguimiento Documentos Entrantes" id="SEGUIMIENTO" title="Catalogo">
			<ice:panelGroup rendered="#{mBeanEntrantesMesa.verCatalogo}">
				<ice:panelGrid columns="6" cellpadding="5">
				<ice:commandButton value="Ver"
						actionListener="#{mBeanEntrantesMesa.eventSeguimiento}" />
				</ice:panelGrid>
			</ice:panelGroup>
		</ice:panelTab>
		-->

			<ice:panelTab label="Arbol de Seguimiento" title="Tree View">
				<ice:panelGroup>
					<br />
					<ice:tree id="tree" value="#{treeControllerMesa.model}" var="node"
						hideRootNode="false" hideNavigation="false"
						imageDir="./xmlhttp/css/rime/css-images/">
						<ice:treeNode>
							<f:facet name="icon">
								<ice:panelGroup style="display: inline">
									<h:graphicImage value="#{node.userObject.icon}" />
								</ice:panelGroup>
							</f:facet>
							<f:facet name="content">
								<ice:panelGroup style="display: inline">
									<ice:commandLink
										actionListener="#{treeControllerMesa.eventGenera}">
										<f:param name="areaId" value="#{node.userObject.area.codigo}" />
										<ice:outputText id="TreeNode" value="#{node.userObject.text}" />
									</ice:commandLink>
								</ice:panelGroup>
							</f:facet>
						</ice:treeNode>
					</ice:tree>

				</ice:panelGroup>
			</ice:panelTab>
		</ice:panelTabSet>

		<ice:menuPopup id="menuPopupEffects">
			<ice:menuItem id="mnuNuevo"
				actionListener="#{mBeanEntrantesMesa.eventoNuevo}" value="Nuevo"
				title="Ingresar Documento">
			</ice:menuItem>
			<ice:menuItem id="mnuModificar" value="Modificar"
				title="Modificar Documento">
			</ice:menuItem>

			<ice:menuItem id="mnuAuditoria" value="Auditoria"
				title="Consultar Documento">
			</ice:menuItem>
		</ice:menuPopup>


	</ice:form>
</body>
</html>
