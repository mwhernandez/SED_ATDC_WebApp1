<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ice="http://www.icesoft.com/icefaces/component">
<head>
<!-- 
columnClasses="ncorrelativo, vnumdoc, vdirigido, vasunto, area, tipodoc, vprioridad, 
		imgvprioridad, dfecregistro, hroingreso, dfecderiva, hrderiva, recep, fecrecep, dias,
		plazo, usuario, estado, imgestado"
 -->

</head>
<body>
	<!--   
	 Ventana de ingreso de Documento entrada
	 Realizado Por : Eli Diaz Horna
	 Fecha Creacion : 24/01/2011
	 Descripcion  : ingreso de Documentos de entrada con el perfil de mesa de partes
	-->

	<ice:form partialSubmit="true">
		<ice:loadBundle basename="com.sedapal.tramite.files.parametros"
			var="msg" />
		<ice:panelTabSet id="icePnlTbSet1"
			tabChangeListener="#{mBeanEntrantesMesa.eventoTab}">

			<!-- Panel Tab 1 defined with some basic content -->
			<ice:panelTab label="Ingreso Documento Mesa Partes" title="Catalogo">
				<ice:panelGroup rendered="#{mBeanEntrantesMesa.verCatalogo}">
					<ice:panelGrid columns="8" cellpadding="5">
						<ice:commandButton value="Nuevo"
							actionListener="#{mBeanEntrantesMesa.eventNuevo}" title="Nuevo"
							image="/images/nuevo2.png"
							style="size: width: 40px; height: 40px" />
						<ice:commandButton value="Eliminar"
							actionListener="#{mBeanEntrantesMesa.eventEliminar}"
							title="Eliminar" image="/images/eliminar2.png"
							style="size: width: 40px; height: 40px" />
						<ice:commandButton value="Actualizar"
							actionListener="#{mBeanEntrantesMesa.eventActualizar}"
							title="Modificar" image="/images/modificar2.png"
							style="size: width: 40px; height: 40px" />
						<ice:outputResource id="pdf-image" shared="false"
							label="Impresión" attachment="false" fileName="doc_mesa.pdf"
							image="/images/impresion.png"
							style="border:none; size: width: 18px; height: 18px"
							mimeType="application/pdf"
							resource="#{mBeanEntrantesMesa.recursoReport}">
						</ice:outputResource>
						<ice:dataExporter for="idProductos" ignorePagination="true"
							label="Excel" type="excel" image="/images/excel2010.jpg"
							style="border:0;"
							popupBlockerLabel="Popup Blocker detected - click here to resume download..."
							clickToCreateFileText="Excel" />
							
						<ice:outputResource id="pdf-image2" shared="false"
							label="Cargo de Recepción" attachment="false"
							fileName="doc_entrada.pdf" image="/images/cargo.png"
							style="width: 9px; height: 9px; border-width: 0px; border-style: none;"
							styleClass="" mimeType="application/pdf"
							resource="#{mBeanEntrantesMesa.recursoReporte}" />
							
						<ice:outputResource id="pdf-image22" shared="false"
							label="Hoja Envio" attachment="false" fileName="doc_hoja.pdf"
							image="/images/impresion2.png"
							style="width: 20px; height: 20px; border-width: 0px; border-style: none;"
							styleClass="" mimeType="application/pdf"
							resource="#{mBeanEntrantesMesa.recursoReporteNuevo}" />
							
													
						<ice:commandButton value="Refrescar"
							actionListener="#{mBeanEntrantesMesa.eventRefrescar}"
							title="Refrescar" image="/images/resert.png"
							style="size: width: 25px; height: 25px">
						</ice:commandButton>
						<ice:outputText value="Nuevo"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Eliminar"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Actualizar"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Impresión"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Excel"
							style="font-size: 10px; color: blue;" />
							
						
						<ice:outputText value="Cargo Recep"
							style="font-size: 10px; color: blue;" />
							
						<ice:outputText value="Línea Digital"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Refrescar"
							style="font-size: 10px; color: blue;" />
					</ice:panelGrid>
					<ice:outputText value="" />
					<ice:outputText value="" />
					<ice:panelGrid columns="1" border="1">
						<ice:panelGrid columns="5" title="Correlativo Impresion"
							styleClass="">
							<ice:outputLabel value="CORRELATIVO INICIAL:"
								style="font-size: 9px; color: green;"></ice:outputLabel>
							<ice:inputText value="#{mBeanEntrantesMesa.corrinicial}"
								onchange="this.value = this.value.toUpperCase();"
								style="font-size: 9px;" size="10" />
							<ice:outputLabel value="CORRELATIVO FINAL :"
								style="font-size: 9px; color: green;"></ice:outputLabel>
							<ice:inputText value="#{mBeanEntrantesMesa.corrfinal}"
								onchange="this.value = this.value.toUpperCase();"
								style="font-size: 9px;" size="10" />
						</ice:panelGrid>
					</ice:panelGrid>
					<ice:panelGrid columns="1" border="1">
						<ice:panelGrid columns="7">
							<ice:outputLabel value="F.INICIAL:"
								style="font-size: 9px; color: green;"></ice:outputLabel>
							<ice:selectInputDate renderMonthAsDropdown="true"
								renderYearAsDropdown="true" value="#{mBeanEntrantesMesa.date1}"
								title="F Inicio" popupDateFormat="dd/MM/yyyy"
								renderAsPopup="true">
								<f:convertDateTime pattern="dd/MM/yyyy"
									timeZone="#{dateSelect.timeZone}" />
							</ice:selectInputDate>

							<ice:outputLabel value="F.FINAL:"
								style="font-size: 9px; color: green;"></ice:outputLabel>
							<ice:selectInputDate renderMonthAsDropdown="true"
								renderYearAsDropdown="true" value="#{mBeanEntrantesMesa.date2}"
								title="F Fin" renderAsPopup="true">
								<f:convertDateTime pattern="dd/MM/yyyy"
									timeZone="#{dateSelect.timeZone}" />
							</ice:selectInputDate>
							<ice:outputLabel value="ESTADO DOC:"
								style="font-size: 9px; color: green;"></ice:outputLabel>
							<ice:selectOneMenu binding="#{mBeanEntrantesMesa.combo}"
								style="font-size: 9px"
								value="#{mBeanEntrantesMesa.item2Seleccionado}" title="Estado">
								<f:selectItems id="slctCreacionUsuario"
									value="#{mBeanEntrantesMesa.items7}" />
							</ice:selectOneMenu>
							<ice:commandButton value="Buscar"
								actionListener="#{mBeanEntrantesMesa.eventFiltros}"
								image="/images/buscar2.png"
								style="size: width: 25px; height: 25px">
							</ice:commandButton>
						</ice:panelGrid>
					</ice:panelGrid>
					<ice:panelGrid columns="7" cellpadding="1" border="1">
						<h:selectOneRadio value="#{mBeanEntrantesMesa.opcion}"
							style="font-size: 9px; color: green;">
							<f:selectItem itemValue="CODIGO" itemLabel="REGISTRO" />
							<f:selectItem itemValue="DESCRIPCION" itemLabel="NRO DOC" />
							<f:selectItem itemValue="NUMEXP" itemLabel="N° EXPEDIENTE" />
							<f:selectItem itemValue="ASUNTO" itemLabel="ASUNTO" />
							<f:selectItem itemValue="REMITE" itemLabel="REMITE" />
							<f:selectItem itemValue="DERIVADO" itemLabel="DERIVADO" />
							<f:selectItem itemValue="TIPO" itemLabel="TIPO" />
							<f:selectItem itemValue="OBSERVACION" itemLabel="OBSERVACION" />
							<f:selectItem itemValue="PRIORIDAD" itemLabel="PRIORIDAD" />
						</h:selectOneRadio>
						<ice:inputText value="#{mBeanEntrantesMesa.detalle}"
							onchange="this.value = this.value.toUpperCase();"
							style="font-size: 9px;" size="70" />
						<ice:commandButton value="Ejecutar"
							actionListener="#{mBeanEntrantesMesa.eventBusquedas}"
							title="Buscar" image="/images/buscar2.png"
							style="size: width: 25px; height: 25px">
						</ice:commandButton>

					</ice:panelGrid>
				</ice:panelGroup>
				<!--   -->
				<ice:panelGroup rendered="#{mBeanEntrantesMesa.verCatalogo}">
					<ice:dataTable var="pro"
						columnClasses="tableCol"
						value="#{mBeanEntrantesMesa.entrantesmesa}"
						style="font-size: 9px;" rows="10"
						sortColumn="#{mBeanEntrantesMesa.sortColumnName}"
						sortAscending="#{mBeanEntrantesMesa.ascending}" id="idProductos"
						scrollable="false">
						<ice:column>
							<ice:rowSelector id="selected" value="#{pro.selected}"
								multiple="#{mBeanEntrantesMesa.multiple}"
								enhancedMultiple="#{mBeanEntrantesMesa.enhancedMultiple}"
								preStyleOnSelection="true" />

							<f:facet name="header">
								<ice:outputText value="Reg" />
								<ice:commandSortHeader columnName="ncorrelativo" arrow="true">
									<ice:outputLabel value="Reg" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText id="idCB" value="#{pro.ncorrelativo}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Num.Doc" />
								<ice:commandSortHeader columnName="vnumdoc" arrow="true">
									<ice:outputLabel value="Num.Doc" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.vnumdoc}" />
						</ice:column>
						<ice:column>
								<f:facet name="header">
									<ice:outputText value="Num.Exp" />
									<ice:commandSortHeader columnName="vnumexp" arrow="true">
										<ice:outputLabel value="Num.Exp" style="color: blue;" />
									</ice:commandSortHeader>
								</f:facet>
								<ice:outputText value="#{pro.vnumexp}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Remitente" />
								<ice:commandSortHeader columnName="vdirigido" arrow="true">
									<ice:outputLabel value="Remitente" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.vdirigido}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Asunto" />
								<ice:commandSortHeader columnName="vasunto" arrow="true">
									<ice:outputLabel value="Asunto" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.vasunto}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Derivado" />
								<ice:commandSortHeader columnName="area" arrow="true">
									<ice:outputLabel value="Derivado" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.area}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Tipo.Doc" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.tipodoc}" />
						</ice:column>
						  
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Prioridad" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.vprioridad}" />
						</ice:column>
						
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="N°Folio" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.nfolio}" />
						</ice:column>
						
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Ingreso" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.vtipoorigen}" />
						</ice:column>
						
						
						<!-- 
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Prio." style="color: blue;" />
							</f:facet>
							<ice:graphicImage id="gi2" alt="#{pro.vprioridad}"
								value="images/#{pro.vprioridad}.png"
								style="size: width: 18px; height: 18px">
							</ice:graphicImage>
						</ice:column>
						-->
					
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec.Ingr" />
								<ice:commandSortHeader columnName="dfecregistro" arrow="true">
									<ice:outputLabel value="Fec.Ingr" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.dfecregistro}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Hr.Ingr" />
								<ice:outputLabel value="Hr.Ingr" style="color: blue" />

							</f:facet>
							<!-- <f:convertDateTime pattern="dd/MM/yyyy HH:mm:ss" /> -->
							<ice:outputText value="#{pro.dhoraregistro}" />

						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec.Rec" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.dfeccre}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Hr.Rec" />
								<ice:outputLabel value="Hr.Rec" style="color: blue" />

							</f:facet>
							<!-- <f:convertDateTime pattern="dd/MM/yyyy HH:mm:ss" /> -->
							<ice:outputText value="#{pro.dhoraderivacion}" />

						</ice:column>
						<!-- 
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Rcp" style="color: blue;" />
							</f:facet>
							<ice:graphicImage id="gi55" alt="#{pro.nrecepcion}"
								value="images/#{pro.nrecepcion}.png"
								style="size: width: 15px; height: 15px">
							</ice:graphicImage>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec.Rcp" />
								<ice:commandSortHeader columnName="dfecharecepcion" arrow="true">
									<ice:outputLabel value="Fec.Rec" style="color: blue;" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.dfecharecepcion}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>
						-->
						
						<!--  
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec.Reg." />
								<ice:commandSortHeader columnName="dfecregistro" arrow="true">
									<ice:outputLabel value="Fec.Reg." style="color: blue;" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.dfecregistro}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>
						-->
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Dia" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.ndiasplazo}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Valor Legal" style="color: blue" />
							</f:facet>
							<ice:graphicImage id="gi578" alt="#{pro.nvalor_legal}"
								value="images/#{pro.nvalor_legal}.png"
								style="size: width: 15px; height: 15px">
							</ice:graphicImage>							
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec.Plazo" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.dfecplazo}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>
						 
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Usuario" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.vrescre}" />
						</ice:column>
						
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Estado" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.vestado}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Est" />
								<ice:commandSortHeader columnName="vestado" arrow="true">
									<ice:outputLabel value="Est" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<h:graphicImage id="gi" alt="#{pro.vestado}"
								value="images/#{pro.vestado}.png"
								style="size: width: 15px; height: 15px">
							</h:graphicImage>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Adj." style="color: blue" />
							</f:facet>
							 
						<ice:outputLink value="#{pro.ubicacion_mesa}" target="_blank" rendered="#{pro.atachment}">
							<h:graphicImage id="gi1" 
								value="images/#{pro.nindicador}.jpg"
								style="border:0; size: width: 25px; height: 25px">
							</h:graphicImage>
						</ice:outputLink>
						
						<!-- 
						<ice:commandButton value="VisualizarPDF"
								actionListener="#{mBeanEntrantesMesa.evenVerPDF}"
								title="Visualizar PDF" image="images/#{pro.nindicador}.jpg"
								style="size: width: 25px; height: 25px" />
						-->

						</ice:column>
						
						

					</ice:dataTable>

					<ice:panelGrid columns="2" style="font-size: 10px">
						<ice:outputText value=" " />
						<!-- Paginator with page controls -->
						<ice:dataPaginator id="dataScroll_3" for="idProductos"
							paginator="true" fastStep="3" paginatorMaxPages="5">
							<f:facet name="first">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-first.gif"
									style="border:none;" title="First Page" />
							</f:facet>
							<f:facet name="last">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-last.gif"
									style="border:none;" title="Last Page" />
							</f:facet>
							<f:facet name="previous">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-previous.gif"
									style="border:none;" title="Previous Page" />
							</f:facet>
							<f:facet name="next">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-next.gif"
									style="border:none;" title="Next Page" />
							</f:facet>
							<f:facet name="fastforward">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-ff.gif"
									style="border:none;" title="Fast Forward" />
							</f:facet>
							<f:facet name="fastrewind">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-fr.gif"
									style="border:none;" title="Fast Backwards" />
							</f:facet>
						</ice:dataPaginator>

						<ice:outputText value="" />

						<!-- Display counts about the table and the currently displayed page -->
						<ice:dataPaginator id="dataScroll_2" for="idProductos"
							style="font-size: 9px;" rowsCountVar="rowsCount"
							displayedRowsCountVar="displayedRowsCount"
							firstRowIndexVar="firstRowIndex" lastRowIndexVar="lastRowIndex"
							pageCountVar="pageCount" pageIndexVar="pageIndex">
							<ice:outputFormat
								value="{0} registros encontrados, mostrando {1}, de {2} hasta {3}. Pagina {4} / {5}."
								styleClass="standard">
								<f:param value="#{rowsCount}" />
								<f:param value="#{displayedRowsCount}" />
								<f:param value="#{firstRowIndex}" />
								<f:param value="#{lastRowIndex}" />
								<f:param value="#{pageIndex}" />
								<f:param value="#{pageCount}" />
							</ice:outputFormat>
						</ice:dataPaginator>

					</ice:panelGrid>
				</ice:panelGroup>
				<ice:panelGroup rendered="#{mBeanEntrantesMesa.verNuevo}">
					<ice:panelGrid columns="1" style="font-size: 9px;" border="1">
						<ice:panelGrid columns="6" style="font-size: 9px;">
							<ice:outputText value="REGISTRO :" />
							<ice:inputText value="#{mBeanEntrantesMesa.ncorrelativoN}"
								disabled="true" />
							<ice:outputText value="AÑO :" />
							<ice:inputText value="#{mBeanEntrantesMesa.nanoN}"
								readonly="True" disabled="true" />
							<ice:outputText value="AREA ORIGEN :" />
							<ice:inputText value="#{mBeanEntrantesMesa.area_origenN}"
								readonly="True" disabled="true" size="50" />

							<ice:outputText value="N° FOLIOS :" />
							<ice:inputText value="#{mBeanEntrantesMesa.nfolioN}" />
							<ice:outputText value="USUARIO CREA :" />
							<ice:inputText value="#{mBeanEntrantesMesa.vrescreN}"
								disabled="true" />
							<ice:outputText value="TIPO INGRESO :" />
							<ice:inputText value="#{mBeanEntrantesMesa.vtipoorigenN}"
								disabled="true" />

							<ice:outputText value="ORIGEN :" />
							<ice:selectOneMenu value="#{mBeanEntrantesMesa.origenN}"
								style="font-size: 9px;">
								<f:selectItem itemValue="2" itemLabel="EXTERNO" />
							</ice:selectOneMenu>
							<ice:outputText value="TIPO DOCUMENTO :" />
							<ice:selectOneMenu value="#{mBeanEntrantesMesa.vtipodocN}"
								style="font-size: 9px;" title="Tipos">
								<f:selectItems value="#{mBeanEntrantesMesa.items2}" />
							</ice:selectOneMenu>
							<ice:outputText value="NUMERO DOC :" />
							<ice:inputText value="#{mBeanEntrantesMesa.vnumdocN}"
								onchange="this.value = this.value.toUpperCase();" size="46"
								maxlength="90" />
							<ice:outputText value="FEC.DOC. :" />
							<ice:selectInputDate renderMonthAsDropdown="true"
								renderYearAsDropdown="true"
								value="#{mBeanEntrantesMesa.dfecdocN}" style="font-size: 9px;"
								title="Fec1" renderAsPopup="true">
								<f:convertDateTime pattern="dd/MM/yyyy"
									timeZone="#{dateSelect.timeZone}" />
							</ice:selectInputDate>
