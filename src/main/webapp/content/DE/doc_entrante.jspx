								scrollable="false">
								<ice:column style="border: none; ">
									<ice:commandButton value="#{anexo.nombreArchivo}"
										actionListener="#{mBeanSeguimiento.eventVerPdfV2(anexo)}"
										title="Visualizar PDF" image="images/1.png"
										style="size: width: 25px; height: 25px" />
									<ice:commandButton value="#{anexo.nombreArchivo}" 
										style="background: none; cursor: pointer; color: #{anexo.selected ? '#0085BC;' : 'black;'} 
										text-decoration: #{anexo.selected ? 'underline;' : 'none;'}"
										actionListener="#{mBeanSeguimiento.eventVerPdfV2(anexo)}" 
										title="Visualizar PDF" />
								</ice:column>
							</ice:dataTable>
							
							<ice:panelGroup styleClass="largeFacet largeFacet-Pdf" rendered="#{mBeanSeguimiento.verPdf2}">
								<ice:panelGrid columns="2">
									<ice:panelTab>
										<ice:panelGroup>
											<!-- file upload usage with progress callback. -->
											<ice:panelGrid>
											  <iframe src="pdfjs/web/viewer.html?file=#{mBeanSeguimiento.rutaPDFPopup}" type="application/pdf" width="750px" height="600px" />							
											</ice:panelGrid>
										</ice:panelGroup>
									</ice:panelTab>
								</ice:panelGrid>
							</ice:panelGroup>
						</ice:panelGrid>
				</ice:panelGroup>
			</ice:panelTab>
	
		</ice:panelTabSet>

		<ice:menuPopup id="menuPopupEffects">
			<ice:menuItem id="mnuNuevo"
				actionListener="#{mBeanEntrantes.eventoNuevo}" value="Nuevo"
				title="Ingresar Documento">
			</ice:menuItem>
			<ice:menuItem id="mnuModificar" value="Modificar"
				title="Modificar Documento">
			</ice:menuItem>

			<ice:menuItem id="mnuAuditoria" value="Auditoria"
				title="Consultar Documento">
			</ice:menuItem>
		</ice:menuPopup>


	</ice:form>
	
	<ice:form style="display: none;">
		<ice:outputLink style="display: none;" styleClass="visualizacionDocumentoButton" value="" target="_blank"></ice:outputLink>
	</ice:form>
</body>
</html>
