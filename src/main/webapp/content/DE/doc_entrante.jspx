								renderAsPopup="true">
								<f:convertDateTime pattern="dd/MM/yyyy"
									timeZone="#{dateSelect.timeZone}" />
							</ice:selectInputDate>
							<ice:outputLabel value="F.FINAL:"
								style="font-size: 9px; color: green;"></ice:outputLabel>
							<ice:selectInputDate renderMonthAsDropdown="true"
								renderYearAsDropdown="true" value="#{mBeanEntrantes.date2}"
								title="F Fin" renderAsPopup="true">
								<f:convertDateTime pattern="dd/MM/yyyy"
									timeZone="#{dateSelect.timeZone}" />
							</ice:selectInputDate>

							<ice:selectOneMenu value="#{mBeanEntrantes.tipoopcion}"
								valueChangeListener="#{mBeanEntrantes.eventtipoconsulta}"
								style="font-size: 9px;" title="Tipo de Consulta">
								<f:selectItems value="#{mBeanEntrantes.itemscombo}" />
							</ice:selectOneMenu>
							<ice:outputLabel value="ESTADO:"
								style="font-size: 9px; color: green;"></ice:outputLabel>
							<ice:selectOneMenu binding="#{mBeanEntrantes.combo}"
