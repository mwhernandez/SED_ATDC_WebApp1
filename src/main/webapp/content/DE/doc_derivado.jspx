									<td><ice:checkbox for="idObservaciones3" index="8" /></td>
									<td><ice:checkbox for="idObservaciones3" index="9" /></td>
								</tr>
								<tr>
									<td><ice:checkbox for="idObservaciones3" index="10" /></td>
									<td><ice:checkbox for="idObservaciones3" index="11" /></td>
									<td><ice:checkbox for="idObservaciones3" index="12" /></td>
									<td><ice:checkbox for="idObservaciones3" index="13" /></td>
									<td><ice:checkbox for="idObservaciones3" index="14" /></td>
								</tr>
							</table>
						</ice:panelGrid>
					</ice:panelGrid>
					<ice:panelGrid columns="3" cellpadding="10">
						<ice:commandButton value="Grabar"
							actionListener="#{mBeanSeguimiento.eventActualizarSeguimiento}"
							title="Grabar" image="/images/grabar.png"
							style="size: width: 35px; height: 35px" />

						<ice:commandButton value="Adjuntar"
							actionListener="#{mBeanSeguimiento.eventDetallesA}"
							title="Adjuntar" image="/images/clip.png"
							style="size: width: 25px; height: 25px" />

						<ice:commandButton value="Regresar"
							actionListener="#{mBeanSeguimiento.eventCancelar}"
							title="Regresar" image="/images/retorno.png"
							style="size: width: 25px; height: 25px" />

						<ice:outputText value="Grabar"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Adjuntar"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Regresar"
							style="font-size: 10px; color: blue;" />
					</ice:panelGrid>
				</ice:panelGroup>

				<!-- Popup para visualizar PDF" -->
				<ice:panelPopup draggable="false" modal="true"
					style="height: 600px; width:830px; color: white; background-color: #0085BC;"
					visible="#{mBeanSeguimiento.verPDF}" autoCentre="true"
					styleClass="icePnlPop">
					<f:facet name="header">
						<ice:panelGroup styleClass="largeFacet">
							<ice:outputText value="Visualizador de PDF"
								style="font-size: 12px; color: blue;"
								styleClass="popupHeaderText" />
						</ice:panelGroup>
					</f:facet>
					<f:facet name="body">
						<ice:panelGroup styleClass="largeFacet">
							<ice:panelGrid columns="2">
								<ice:panelTab>
									<ice:panelGroup>
										<!-- file upload usage with progress callback. -->
										<ice:panelGrid>
											<iframe
												src="pdfjs/web/viewer.html?file=#{mBeanSeguimiento.rutaPDFPopup}"
												type="application/pdf" width="800px" height="500px" />
										</ice:panelGrid>
									</ice:panelGroup>
								</ice:panelTab>
							</ice:panelGrid>
							<ice:commandButton type="submit" value="Cancelar"
								image="/images/cerrar.png"
								style="size: width: 35px; height: 35px; margin: 0 auto; display: block;"
								actionListener="#{mBeanSeguimiento.cerrarDetalles}" />
						</ice:panelGroup>
					</f:facet>
				</ice:panelPopup>

				<!-- Popup para cargar un archivo adjunto seguimiento" />-->
				<ice:panelPopup draggable="false" modal="true"
					style="width: 600px; height: 400px; color: white; background-color: #0085BC;"
					visible="#{mBeanSeguimiento.verDetalles}" autoCentre="true"
					styleClass="icePnlPop">
					<f:facet name="header">
						<ice:panelGroup styleClass="popupHeaderWrapper">
							<ice:outputText value="Archivo Adjunto Seguimiento"
								styleClass="popupHeaderText"
								style="font-size: 12px; color: blue;" />
						</ice:panelGroup>
					</f:facet>
					<f:facet name="body">
						<ice:panelGroup styleClass="popupBody">
							<ice:panelGrid columns="2">
								<ice:panelTab label="Carga de Datos" title="Carga de Datos">
									<ice:panelGroup>
										<!-- file upload usage with progress callback. -->
										<ice:panelGrid>
											<ice:outputText value="Seleccione un archivo a cargar:"
												style="color: white;font: bold;" />
											<ice:inputFile id="inputFileName3" uniqueFolder="false"
												uploadDirectoryAbsolute="true" autoUpload="true"
												uploadDirectory="#{msg['path.seguimiento']}"
												progressListener="#{mCarga.fileUploadProgress}"
												actionListener="#{mCarga.uploadFile}" />
											<!-- progress bar, percent value is upated via progressListener-->

											<ice:outputProgress value="#{mCarga.fileProgress}"
												styleClass="uploadProgressBar" />
											<ice:message for="inputFileName" />
											<br />
											<ice:outputText value="#{mCarga.componentStatus}"
												style="color: white;font: bold;" />

										</ice:panelGrid>


									</ice:panelGroup>
								</ice:panelTab>

							</ice:panelGrid>
							<ice:commandButton id="modalPnlCBError" type="submit"
								value="Salir" image="/images/retorno.png"
								style="size: width: 20px; height: 20px"
								actionListener="#{mBeanSeguimiento.cerrarDetalles}" />
						</ice:panelGroup>
					</f:facet>
				</ice:panelPopup>
				<!-- Alerta de mensaje -->
				<ice:panelPopup draggable="false" modal="true"
					style="width: 270px; height: 160px; background-color: #0085BC;"
					visible="#{mBeanSeguimiento.ver}" autoCentre="true"
					styleClass="icePnlPop">
					<f:facet name="header">
						<ice:panelGroup styleClass="popupHeaderWrapper">
							<ice:outputText value="Mensaje"
								style="font-size: 12px; color: blue;" />
						</ice:panelGroup>
					</f:facet>
					<f:facet name="body">
						<ice:panelGrid columns="1" style="text-align: center;">
							<ice:outputText value="#{mBeanSeguimiento.error}"
								style="font-size: 12px; color: white;" />
							<br />
							<ice:commandButton type="submit" value="Aceptar"
								image="/images/cerrar.png"
								style="size: width: 35px; height: 35px"
								actionListener="#{mBeanSeguimiento.cerrar}" />
						</ice:panelGrid>
					</f:facet>
				</ice:panelPopup>
			</ice:panelTab>

			<ice:panelTab label="Arbol de Seguimiento" title="Tree View">
				<ice:panelGroup>
					<br />
					<ice:tree id="tree" value="#{treeControllerDerivador.model}"
						var="node" hideRootNode="false" hideNavigation="false"
						imageDir="./xmlhttp/css/rime/css-images/">
						<ice:treeNode>
							<f:facet name="icon">
								<ice:panelGroup style="display: inline">
									<h:graphicImage value="#{node.userObject.icon}" />
								</ice:panelGroup>
							</f:facet>
							<f:facet name="content">
								<ice:panelGroup style="display: inline">
									<ice:commandLink
										actionListener="#{treeControllerDerivador.eventGenera}">
										<f:param name="areaId" value="#{node.userObject.area.codigo}" />
										<ice:outputText id="TreeNode" value="#{node.userObject.text}" />
									</ice:commandLink>
								</ice:panelGroup>
							</f:facet>
						</ice:treeNode>
					</ice:tree>

				</ice:panelGroup>
			</ice:panelTab>

		</ice:panelTabSet>

		<ice:menuPopup id="menuPopupEffects">
			<ice:menuItem id="mnuNuevo"
				actionListener="#{mBeanEntrantesDerivado.eventoNuevo}" value="Nuevo"
				title="Ingresar Documento">
			</ice:menuItem>
			<ice:menuItem id="mnuModificar" value="Modificar"
				title="Modificar Documento">
			</ice:menuItem>

			<ice:menuItem id="mnuAuditoria" value="Auditoria"
				title="Consultar Documento">
			</ice:menuItem>
		</ice:menuPopup>


	</ice:form>
</body>
</html>
