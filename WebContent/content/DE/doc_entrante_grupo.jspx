<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ice="http://www.icesoft.com/icefaces/component">
<body>
	<!-- Ventana de Documento entrada
	 Realizado Por : Eli Diaz Horna
	 Fecha Creacion : 24/01/2011
	 Descripcion  : Documentos de entrada con el perfil grupo para la alta direccion
	-->

	<ice:form partialSubmit="true">
		<ice:loadBundle basename="com.sedapal.tramite.files.parametros"
			var="msg" />
		<ice:panelTabSet id="icePnlTbSet1"
			tabChangeListener="#{mBeanEntrantesGrupo.eventoTab}">

			<!-- Panel Tab 1 defined with some basic content -->
			<ice:panelTab label="Documentos Entrantes Grupo" title="Catalogo">
				<ice:panelGroup rendered="#{mBeanEntrantesGrupo.verCatalogo}">
					<ice:panelGrid columns="8" cellpadding="5">
						<!-- 
					<ice:commandButton value="Email"
						actionListener="#{mBeanEntrantes.eventEnviarCorreo}" title="Email"
						image="/images/mail1.jpg" style="size: width: 25px; height: 25px" />
					 -->
						<ice:commandButton value="Nuevo"
							actionListener="#{mBeanEntrantesGrupo.eventNuevo}" title="Nuevo"
							image="/images/nuevo2.png"
							style="size: width: 40px; height: 40px" />
						<ice:commandButton value="Eliminar"
							actionListener="#{mBeanEntrantesGrupo.eventEliminar}"
							title="Eliminar" image="/images/eliminar2.png"
							style="size: width: 40px; height: 40px" />
						<ice:commandButton value="Actualizar"
							actionListener="#{mBeanEntrantesGrupo.eventActualizar}"
							title="Modificar" image="/images/modificar2.png"
							style="size: width: 40px; height: 40px" />
						<ice:commandButton value="Alerta"
							actionListener="#{mBeanEntrantesGrupo.eventAlerta}"
							title="Modificar" image="/images/alerta.png"
							style="size: width: 32px; height: 32px" />

						<ice:outputResource id="pdf-image" shared="false"
							label="Impresión" attachment="false" fileName="doc_entrada.pdf"
							image="/images/impresion.png"
							style="border:none; size: width: 18px; height: 18px"
							mimeType="application/pdf"
							resource="#{mBeanEntrantesGrupo.recursoReport}">
						</ice:outputResource>
						<ice:dataExporter for="idProductos" ignorePagination="true"
							label="Excel" type="excel" image="/images/excel2010.jpg"
							style="border:0;"
							popupBlockerLabel="Popup Blocker detected - click here to resume download..."
							clickToCreateFileText="Excel" />

						<ice:outputResource id="pdf-image2" shared="false"
							label="Hoja Envio" attachment="false" fileName="doc_entrada.pdf"
							image="/images/hojaenvio.png"
							style="width: 18px; height: 18px; border-width: 0px; border-style: none;"
							styleClass="" mimeType="application/pdf"
							resource="#{mBeanEntrantesGrupo.recursoReporte}" />

						<ice:commandButton value="Refrescar"
							actionListener="#{mBeanEntrantesGrupo.eventRefrescar}"
							title="Refrescar" image="/images/resert.png"
							style="size: width: 25px; height: 25px">
						</ice:commandButton>

						<ice:outputText value="Nuevo"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Eliminar"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Actualizar"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Alerta"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Impresión"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Excel"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Hoja Envio"
							style="font-size: 10px; color: blue;" />

						<ice:outputText value="Refrescar"
							style="font-size: 10px; color: blue;" />

					</ice:panelGrid>

					<ice:panelGrid columns="3" border="1">
						<ice:panelGrid columns="10">
							<ice:outputLabel value="F.INICIAL:"
								style="font-size: 9px; color: green;"></ice:outputLabel>
							<ice:selectInputDate renderMonthAsDropdown="true"
								renderYearAsDropdown="true" value="#{mBeanEntrantesGrupo.date1}"
								title="F Inicio" popupDateFormat="dd/MM/yyyy"
								renderAsPopup="true">
								<f:convertDateTime pattern="dd/MM/yyyy"
									timeZone="#{dateSelect.timeZone}" />
							</ice:selectInputDate>

							<ice:outputLabel value="F.FINAL:"
								style="font-size: 9px; color: green;"></ice:outputLabel>
							<ice:selectInputDate renderMonthAsDropdown="true"
								renderYearAsDropdown="true" value="#{mBeanEntrantesGrupo.date2}"
								title="F Fin" renderAsPopup="true">
								<f:convertDateTime pattern="dd/MM/yyyy"
									timeZone="#{dateSelect.timeZone}" />
							</ice:selectInputDate>
							<ice:selectOneMenu value="#{mBeanEntrantesGrupo.tipoopcion}"
								valueChangeListener="#{mBeanEntrantesGrupo.eventtipoconsulta}"
								style="font-size: 9px;" title="Tipo de Consulta">
								<f:selectItems value="#{mBeanEntrantesGrupo.itemscombo}" />
							</ice:selectOneMenu>
							<ice:outputLabel value="ESTADO:"
								style="font-size: 9px; color: green;"></ice:outputLabel>
							<ice:selectOneMenu binding="#{mBeanEntrantesGrupo.combo}"
								disabled="#{mBeanEntrantesGrupo.disverestado}"
								style="font-size: 9px"
								value="#{mBeanEntrantesGrupo.item2Seleccionado}" title="Tipos">
								<f:selectItems id="slctCreacionUsuario"
									value="#{mBeanEntrantesGrupo.items7}" />
							</ice:selectOneMenu>
							<ice:outputLabel value="REMITENTE:"
								style="font-size: 9px; color: green;"></ice:outputLabel>
							<ice:selectOneMenu binding="#{mBeanEntrantesGrupo.comboarea}"
								disabled="#{mBeanEntrantesGrupo.disverremite}"
								style="font-size: 9px"
								value="#{mBeanEntrantesGrupo.itemareaSeleccionado}"
								title="Remitente">
								<f:selectItems id="slctCreacionUsuario"
									value="#{mBeanEntrantesGrupo.items10}" />
							</ice:selectOneMenu>
						</ice:panelGrid>
						<ice:commandButton value="Buscar"
							actionListener="#{mBeanEntrantesGrupo.eventFiltros}"
							image="/images/buscar2.png"
							style="size: width: 25px; height: 25px">
						</ice:commandButton>
						<ice:outputResource id="pdf-image3" shared="false"
							label="Impresión Total" attachment="false"
							fileName="doc_impresion.pdf"
							image="/images/impresion.png; border: none;"
							style="border: none;" styleClass="border-style: none;"
							mimeType="application/pdf"
							resource="#{mBeanEntrantesGrupo.recursoReporte}" />
					</ice:panelGrid>

					<ice:panelGrid columns="7" cellpadding="1" border="1">
						<h:selectOneRadio value="#{mBeanEntrantesGrupo.opcion}"
							style="font-size: 9px; color: green;">
							<f:selectItem itemValue="CODIGO" itemLabel="REGISTRO" />
							<f:selectItem itemValue="NRO_DOC" itemLabel="NRO DOC" />
							<f:selectItem itemValue="NUMEXP" itemLabel="N° EXPEDIENTE" />
							<f:selectItem itemValue="REMITENTE" itemLabel="REMITENTE" />
							<f:selectItem itemValue="ASUNTO" itemLabel="ASUNTO" />
							<f:selectItem itemValue="DIRIGIDO" itemLabel="DIRIGIDO" />
							<f:selectItem itemValue="OBSERVACION" itemLabel="OBSERVACION" />
							<f:selectItem itemValue="REFERENCIA" itemLabel="REFERENCIA" />
							<f:selectItem itemValue="TIPO" itemLabel="TIPO DOC." />
							<f:selectItem itemValue="PRIORIDAD" itemLabel="PRIORIDAD" />
							<f:selectItem itemValue="SEGUIMIENTO" itemLabel="SEGUIMIENTO" />
						</h:selectOneRadio>
						<ice:inputText value="#{mBeanEntrantesGrupo.detalle}"
							onchange="this.value = this.value.toUpperCase();"
							style="font-size: 9px;" size="70" />
						<ice:commandButton value="Ejecutar"
							actionListener="#{mBeanEntrantesGrupo.eventBusquedas}"
							title="Buscar" image="/images/buscar2.png"
							style="size: width: 25px; height: 25px">
						</ice:commandButton>
					</ice:panelGrid>
				</ice:panelGroup>

				<ice:panelGroup rendered="#{mBeanEntrantesGrupo.verCatalogo}">
					<ice:dataTable var="pro" value="#{mBeanEntrantesGrupo.entrantes}"
						columnClasses="tableCol" style="font-size: 9px;" rows="10"
						sortColumn="#{mBeanEntrantesGrupo.sortColumnName}"
						sortAscending="#{mBeanEntrantesGrupo.ascending}" id="idProductos"
						scrollable="false">
						<!--
						<ice:column>
							<ice:rowSelector id="selected" value="#{pro.selected}"
								multiple="#{mBeanEntrantes.multiple}"
								enhancedMultiple="#{mBeanEntrantes.enhancedMultiple}"
								preStyleOnSelection="true" />

							<f:facet name="header">
								<ice:outputText value="Ano" />
								<ice:commandSortHeader columnName="nano" arrow="true">
									<ice:outputLabel value="Ano" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText id="idCB" value="#{pro.nano}" />
						</ice:column>
						-->
						<ice:column>
							<ice:rowSelector id="selected" value="#{pro.selected}"
								multiple="#{mBeanEntrantesGrupo.multiple}"
								enhancedMultiple="#{mBeanEntrantesGrupo.enhancedMultiple}"
								preStyleOnSelection="true" />
							<f:facet name="header">
								<ice:outputText value="Reg" />
								<ice:commandSortHeader columnName="ncorrelativo" arrow="true">
									<ice:outputLabel value="Reg" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText id="idCB" value="#{pro.ncorrelativo}" />
						</ice:column>

						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Num.Doc" />
								<ice:commandSortHeader columnName="vnumdoc" arrow="true">
									<ice:outputLabel value="Num.Doc" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.vnumdoc}" />
						</ice:column>
						<ice:column>
								<f:facet name="header">
									<ice:outputText value="Num.Exp" />
									<ice:commandSortHeader columnName="vnumexp" arrow="true">
										<ice:outputLabel value="Num.Exp" style="color: blue;" />
									</ice:commandSortHeader>
								</f:facet>
								<ice:outputText value="#{pro.vnumexp}" />
							</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Remitente" />
								<ice:commandSortHeader columnName="vdirigido" arrow="true">
									<ice:outputLabel value="Remitente" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.vdirigido}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Asunto" />
								<ice:commandSortHeader columnName="vasunto" arrow="true">
									<ice:outputLabel value="Asunto" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.vasunto}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Dirigido" />
								<ice:commandSortHeader columnName="area" arrow="true">
									<ice:outputLabel value="Dirigido" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.area}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Tipo Doc" />
								<ice:commandSortHeader columnName="tipodoc" arrow="true">
									<ice:outputLabel value="Tipo Doc" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.tipodoc}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Prio." style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.vprioridad}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Prio." style="color: blue;" />
							</f:facet>
							<ice:graphicImage id="gi2" alt="#{pro.vprioridad}"
								value="images/#{pro.vprioridad}.png"
								style="size: width: 18px; height: 18px">
							</ice:graphicImage>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec.Ingr" />
								<ice:commandSortHeader columnName="dfecregistro" arrow="true">
									<ice:outputLabel value="Fec.Ingr" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.dfecregistro}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Hr.Ingr" />
								<ice:outputLabel value="Hr.Ingr" style="color: blue" />

							</f:facet>
							<!-- <f:convertDateTime pattern="dd/MM/yyyy HH:mm:ss" /> -->
							<ice:outputText value="#{pro.dhoraregistro}" />

						</ice:column>
						<!--  
						<ice:column style="text-align: center;">
							<f:facet name="header">
								<ice:outputLabel value="Fec.Doc" />
								<ice:commandSortHeader columnName="dfecdoc" arrow="true">
									<ice:outputLabel value="Fec.Doc." style="color: blue;" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText id="dfecdoc" value="#{pro.dfecdoc}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Hora.Doc." />
								<ice:outputLabel value="Hora.Doc." style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.dthoradoc}" />
						</ice:column>
						-->
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec.Deriv" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.dfeccre}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>

						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Hr.Deriv" />
								<ice:outputLabel value="Hr.Deriv" style="color: blue" />

							</f:facet>
							<!-- <f:convertDateTime pattern="dd/MM/yyyy HH:mm:ss" /> -->
							<ice:outputText value="#{pro.dhoraderivacion}" />

						</ice:column>

						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec.Recep." />
								<ice:commandSortHeader columnName="dfecregistro" arrow="true">
									<ice:outputLabel value="Fec.Recep." style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.dfecregistro}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Dias" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.ndiasplazo}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec. Plazo" />
								<ice:commandSortHeader columnName="dfecplazo" arrow="true">
									<ice:outputLabel value="Fec. Plazo" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.dfecplazo}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec.Linea" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.dfechalinea}">
									<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
																					
						</ice:column>
						<ice:column>
								<f:facet name="header">
									<ice:outputText value="Hr.Linea" />
									<ice:outputLabel value="Hr.Linea" style="color: blue" />

								</f:facet>
								<!-- <f:convertDateTime pattern="dd/MM/yyyy HH:mm:ss" /> -->
								<ice:outputText value="#{pro.dhoralinea}" />

						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Estado" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.vestado}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Est" />
								<ice:commandSortHeader columnName="vestado" arrow="true">
									<ice:outputLabel value="Est" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:graphicImage id="gi" alt="#{pro.vestado}"
								value="images/#{pro.vestado}.png"
								style="size: width: 15px; height: 15px">
							</ice:graphicImage>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Adj." style="color: blue" />
							</f:facet>
							
							<!-- ELI COMENTA EN ANTERIOR VISUALIZADOR  27/10/2020-->
							 
							<ice:outputLink value="#{pro.ubicacion_entrada}" 
								target="_blank"
								rendered="#{pro.atachment}">
								<ice:graphicImage id="gi1" 
									value="images/#{pro.nindicador}.jpg"
									style="border:0; size: width: 25px; height: 25px">
								</ice:graphicImage>
							</ice:outputLink>
							
														
							<!--
							<input type="text" style="display: none;"
								value="#{pro.ubicacion_entrada}" id="rutaPDF#{pro.ncorrelativo}"></input>
								
							<ice:commandButton onclick="openLink(#{pro.ncorrelativo});"
								value="VisualizarPDF"
								actionListener="#{mBeanEntrantesGrupo.evenVerPDF}"
								title="Visualizar PDF" image="images/#{pro.nindicador}.jpg"
								style="size: width: 25px; height: 25px" />
							-->
							<!--  
							<input type="text" style="display:none;" value="#{pro.ubicacion_entrada}" id="rutaPDF"></input>				  
								<ice:commandButton onclick="openLink();" value="VisualizarPDF"
								actionListener="#{mBeanEntrantesGrupo.evenVerPDF}"
								title="Visualizar PDF" image="images/#{pro.nindicador}.jpg"
								style="size: width: 25px; height: 25px" />
							
							  	
					        <ice:commandButton value="VisualizarPDF"
								actionListener="#{mBeanEntrantesGrupo.evenVerPDF}"
								title="Visualizar PDF" image="images/#{pro.nindicador}.jpg"
								style="size: width: 25px; height: 25px" />
							-->
							
						</ice:column>


					</ice:dataTable>
					<ice:panelGrid columns="2" style="font-size: 10px">
						<ice:outputText value=" " />
						<!-- Paginator with page controls -->
						<ice:dataPaginator id="dataScroll_3" for="idProductos"
							paginator="true" fastStep="3" paginatorMaxPages="5">
							<f:facet name="first">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-first.gif"
									style="border:none;" title="First Page" />
							</f:facet>
							<f:facet name="last">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-last.gif"
									style="border:none;" title="Last Page" />
							</f:facet>
							<f:facet name="previous">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-previous.gif"
									style="border:none;" title="Previous Page" />
							</f:facet>
							<f:facet name="next">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-next.gif"
									style="border:none;" title="Next Page" />
							</f:facet>
							<f:facet name="fastforward">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-ff.gif"
									style="border:none;" title="Fast Forward" />
							</f:facet>
							<f:facet name="fastrewind">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-fr.gif"
									style="border:none;" title="Fast Backwards" />
							</f:facet>
						</ice:dataPaginator>

						<ice:outputText value="" />

						<!-- Display counts about the table and the currently displayed page -->
						<ice:dataPaginator id="dataScroll_2" for="idProductos"
							style="font-size: 9px;" rowsCountVar="rowsCount"
							displayedRowsCountVar="displayedRowsCount"
							firstRowIndexVar="firstRowIndex" lastRowIndexVar="lastRowIndex"
							pageCountVar="pageCount" pageIndexVar="pageIndex">
							<ice:outputFormat
								value="{0} registros encontrados, mostrando {1}, de {2} hasta {3}. Pagina {4} / {5}."
								styleClass="standard">
								<f:param value="#{rowsCount}" />
								<f:param value="#{displayedRowsCount}" />
								<f:param value="#{firstRowIndex}" />
								<f:param value="#{lastRowIndex}" />
								<f:param value="#{pageIndex}" />
								<f:param value="#{pageCount}" />
							</ice:outputFormat>
						</ice:dataPaginator>

					</ice:panelGrid>

				</ice:panelGroup>
				<ice:panelGroup rendered="#{mBeanEntrantesGrupo.verNuevo}">
					<ice:panelGrid columns="1" style="font-size: 9px;" border="1">
						<ice:panelGrid columns="6" style="font-size: 9px;">
							<ice:outputText value="REGISTRO :" />
							<ice:inputText value="#{mBeanEntrantesGrupo.ncorrelativoN}"
								disabled="true" />
							<ice:outputText value="AÑO :" />
							<ice:inputText value="#{mBeanEntrantesGrupo.nanoN}"
								readonly="True" disabled="true" />
							<ice:outputText value="AREA ORIGEN :" />
							<ice:inputText value="#{mBeanEntrantesGrupo.area_origenN}"
								readonly="True" disabled="true" size="50" />

							<ice:outputText value="N° FOLIOS :" />
							<ice:inputText value="#{mBeanEntrantesGrupo.nfolioN}" />
							<ice:outputText value="USUARIO CREA :" />
