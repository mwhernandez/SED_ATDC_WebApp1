<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ice="http://www.icesoft.com/icefaces/component">
<body>
<!-- Ventana de Consulta de Documento de Entrada
	 Realizado Por : Eli Diaz Horna
	 Fecha Creacion : 24/01/2011
	 Descripcion  : Consulta de Documentos Entrada con el perfil consulta
	-->
	<ice:form partialSubmit="true">
		<ice:loadBundle basename="com.sedapal.tramite.files.parametros"
			var="msg" />
		<ice:panelTabSet id="icePnlTbSet1"
			tabChangeListener="#{mBeanEntrantes.eventoTab}">

			<!-- Panel Tab 1 defined with some basic content -->
			<ice:panelTab label="Consulta Documentos Entrantes" title="Catalogo">
				<ice:panelGroup rendered="#{mBeanEntrantes.verCatalogo}">
					<ice:panelGrid columns="10" cellpadding="5">						
						<ice:commandButton value="Actualizar"
							actionListener="#{mBeanEntrantes.eventActualizar}"
							title="Modificar" image="/images/modificar2.png"
							style="size: width: 40px; height: 40px" />						
						<ice:outputResource 
							id="pdf-image"
							shared="false" 							
							label="Impresión"
							attachment="false"						    
						    fileName="doc_entrada.pdf"
							image="/images/impresion.png; border: none;"
							style="border: none;"
							styleClass="border-style: none;"
							mimeType="application/pdf"
							resource="#{mBeanEntrantes.recursoReport}" />
						<ice:dataExporter for="idProductos" ignorePagination="true"
									label="Excel" 
									type="excel"									
									image="/images/excel2010.jpg"
									style="border:none;"
									styleClass="size: width: 30px; height: 30px"								
									popupBlockerLabel="Popup Blocker detected - click here to resume download..."																									
									clickToCreateFileText="Excel"/>	
						
						<ice:outputResource id="pdf-image2"
							shared="false" 							
							label="Hoja Envio"
							attachment="false"						    
						    fileName="doc_entrada.pdf"
							image="/images/hojaenvio.png"
							style="width: 18px; height: 18px; border-width: 0px; border-style: none;"
							styleClass=""
							mimeType="application/pdf"
							resource="#{mBeanEntrantes.recursoReporte}" />						 	 				
						<ice:commandButton value="Refrescar"
							actionListener="#{mBeanEntrantes.eventRefrescar}"
							title="Refrescar" image="/images/resert.png"
							style="size: width: 25px; height: 25px">
						</ice:commandButton>
						</ice:panelGrid>
						<ice:panelGrid columns="14">						
						<ice:outputText value="Actualizar" style="font-size: 10px; color: blue;"/>					
						<ice:outputText value=" " />
						<ice:outputText value="Impresión" style="font-size: 10px; color: blue;"/>					
						<ice:outputText value=" " />
						<ice:outputText value="Excel" style="font-size: 10px; color: blue;"/>					
						<ice:outputText value=" " />								
						<ice:outputText value="Hoja Envio"  style="font-size: 10px; color: blue;"/>										
						<ice:outputText value=" "  />
						<ice:outputText value="Refrescar"  style="font-size: 10px; color: blue;"/>
						</ice:panelGrid>
					
					<ice:panelGrid columns="3" border="1">
					<ice:panelGrid columns="7">
						<ice:outputLabel value="F.INICIAL:" style="font-size: 9px; color: blue"></ice:outputLabel>
						<ice:selectInputDate renderMonthAsDropdown="true"
							renderYearAsDropdown="true" value="#{mBeanEntrantes.date1}"
							title="F Inicio" popupDateFormat="dd/MM/yyyy"							
							renderAsPopup="true">
							<f:convertDateTime pattern="dd/MM/yyyy"
								timeZone="#{dateSelect.timeZone}" />
						</ice:selectInputDate>

						<ice:outputLabel value="F.FINAL:" style="font-size: 9px; color: blue"></ice:outputLabel>
						<ice:selectInputDate renderMonthAsDropdown="true"
							renderYearAsDropdown="true" value="#{mBeanEntrantes.date2}"
							title="F Fin" renderAsPopup="true">
							<f:convertDateTime pattern="dd/MM/yyyy"
								timeZone="#{dateSelect.timeZone}" />
						</ice:selectInputDate>
						<ice:outputLabel value="ESTADO DOC:" style="font-size: 9px; color: blue"></ice:outputLabel>
						<ice:selectOneMenu binding="#{mBeanEntrantes.combo}"
							style="font-size: 9px"
							value="#{mBeanEntrantes.item2Seleccionado}" title="Tipos">
							<f:selectItems id="slctCreacionUsuario"
								value="#{mBeanEntrantes.items7}" />
						</ice:selectOneMenu>
						<ice:commandButton value="Buscar"
							actionListener="#{mBeanEntrantes.eventFiltros}"
							image="/images/buscar2.png"
							style="size: width: 25px; height: 25px">
						</ice:commandButton>

					</ice:panelGrid>
					</ice:panelGrid>					
					<ice:panelGrid columns="7" cellpadding="1" border="1">
						<h:selectOneRadio value="#{mBeanEntrantes.opcion}"
							style="font-size: 9px;color: blue">
							<f:selectItem itemValue="CODIGO" itemLabel="REGISTRO" />
							<f:selectItem itemValue="DESCRIPCION" itemLabel="NRO DOC" />
							<f:selectItem itemValue="REMITENTE" itemLabel="REMITENTE" />
							<f:selectItem itemValue="ASUNTO" itemLabel="ASUNTO" />
							<f:selectItem itemValue="OBSERVACION" itemLabel="OBSERVACION" />
							<f:selectItem itemValue="REFERENCIA" itemLabel="REFERENCIA" />
							<f:selectItem itemValue="TIPO" itemLabel="TIPO DOC." />
							<!-- <f:selectItem itemValue="SEGUIMIENTO" itemLabel="SEGUIMIENTO" /> -->
						</h:selectOneRadio>
						<ice:inputText value="#{mBeanEntrantes.detalle}"
							onchange="this.value = this.value.toUpperCase();"
							style="font-size: 9px;" size="70" />
						<ice:commandButton value="Ejecutar"
							actionListener="#{mBeanEntrantes.eventBusquedas}" title="Buscar"
							image="/images/buscar2.png"
							style="size: width: 25px; height: 25px">
						</ice:commandButton>
					</ice:panelGrid>
				</ice:panelGroup>
				
				<ice:panelGroup rendered="#{mBeanEntrantes.verCatalogo}">
					<ice:panelGrid columns="10" style="font-size: 9px;">
					<ice:dataTable var="pro" value="#{mBeanEntrantes.entrantes}"					
						columnClasses="tableCol" style="font-size: 9px;" rows="10" 
						sortColumn="#{mBeanEntrantes.sortColumnName}"
						sortAscending="#{mBeanEntrantes.ascending}" 
						id="idProductos" scrollable="false">
									
						<ice:column>
							<ice:rowSelector value="#{pro.selected}"
								multiple="#{mBeanEntrantes.multiple}"
								enhancedMultiple="#{mBeanEntrantes.enhancedMultiple}"
								preStyleOnSelection="true" />

							<f:facet name="header">
								<ice:outputText value="Reg" />
								<ice:commandSortHeader columnName="ncorrelativo" arrow="true">
									<ice:outputLabel value="Reg" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText id="idCB" value="#{pro.ncorrelativo}" />
						</ice:column>
															
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Num.Doc" style="color: blue" />
								<ice:commandSortHeader columnName="vnumdoc" arrow="true">
									<ice:outputLabel value="Num.Doc" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.vnumdoc}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Remitente" style="color: blue" />
								<ice:commandSortHeader columnName="vdirigido" arrow="true">
									<ice:outputLabel value="Remitente" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.vdirigido}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Asunto" style="color: blue" />
								<f:facet name="header">
								<ice:outputText value="Asunto" />
								<ice:commandSortHeader columnName="vasunto" arrow="true">
									<ice:outputLabel value="Asunto" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							</f:facet>
							<ice:outputText value="#{pro.vasunto}" />
						</ice:column>
						<!-- 					 
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Dirigido" />
						</f:facet>
						<ice:outputText value="#{pro.area}" />
					</ice:column>
					 -->
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Tipo Doc" style="color: blue"/>
								 <ice:commandSortHeader columnName="tipodoc" arrow="true">
									<ice:outputLabel value="Tipo Doc" style="color: blue"/>
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.tipodoc}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Prio." style="color: blue"/>
							</f:facet>
							<ice:outputText value="#{pro.vprioridad}" />
						</ice:column>
						<ice:column style="text-align: center;">
							<f:facet name="header">
								<ice:outputLabel value="Fec.Doc" style="color: blue"/>
			                    	<ice:commandSortHeader columnName="dfecdoc" arrow="true">
									<ice:outputLabel value="Fec.Doc." style="color: blue" />
							</ice:commandSortHeader>
							</f:facet>
							<ice:outputText id="dfecdoc" value="#{pro.dfecdoc}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec.Recep." style="color: blue" />
								<ice:commandSortHeader columnName="dfecregistro" arrow="true">
									<ice:outputLabel value="Fec.Recep." style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.dfecregistro}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Dias" style="color: blue" />
							</f:facet>
							<ice:outputText value="#{pro.ndiasplazo}" />
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Fec.Plazo" style="color: blue" />
								<ice:commandSortHeader columnName="dfecplazo" arrow="true">
									<ice:outputLabel value="Fec.Plazo" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:outputText value="#{pro.dfecplazo}">
								<f:convertDateTime pattern="dd/MM/yyyy" />
							</ice:outputText>
						</ice:column>						
						<ice:column>
						<f:facet name="header">
							<ice:outputText value="Estado" style="color: blue" />
						</f:facet>
						<ice:outputText value="#{pro.vestado}" />
						</ice:column>					
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Est" style="color: blue" />
								<ice:commandSortHeader columnName="vestado" arrow="true">
									<ice:outputLabel value="Est" style="color: blue" />
								</ice:commandSortHeader>
							</f:facet>
							<ice:graphicImage id="gi" alt="#{pro.vestado}"
								value="images/#{pro.vestado}.png"
								style="size: width: 15px; height: 15px">
							</ice:graphicImage>
						</ice:column>
						<ice:column>
							<f:facet name="header">
								<ice:outputText value="Adj." style="color: blue" />
							</f:facet>
							<ice:outputLink value="#{pro.ubicacion_entrada}" target="_blank"  
								rendered="#{pro.atachment}">
								<ice:graphicImage id="gi1" value="images/#{pro.nindicador}.jpg"
									style="border:0; size: width: 25px; height: 25px">
								</ice:graphicImage>
							</ice:outputLink>
						</ice:column>
						<!-- 
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Ver" />
						</f:facet>
						<ice:outputLink value="#{pro.vubiarchivo}" target="_blank">Ver</ice:outputLink>
					</ice:column>
					 -->
					</ice:dataTable>
					</ice:panelGrid>
					<ice:panelGrid columns="2" style="font-size: 10px">
						<ice:outputText value=" " />
						<!-- Paginator with page controls -->
						<ice:dataPaginator id="dataScroll_3" for="idProductos"
							paginator="true" fastStep="3" paginatorMaxPages="5">
							<f:facet name="first">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-first.gif"
									style="border:none;" title="First Page" />
							</f:facet>
							<f:facet name="last">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-last.gif"
									style="border:none;" title="Last Page" />
							</f:facet>
							<f:facet name="previous">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-previous.gif"
									style="border:none;" title="Previous Page" />
							</f:facet>
							<f:facet name="next">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-next.gif"
									style="border:none;" title="Next Page" />
							</f:facet>
							<f:facet name="fastforward">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-ff.gif"
									style="border:none;" title="Fast Forward" />
							</f:facet>
							<f:facet name="fastrewind">
								<ice:graphicImage
									url="./xmlhttp/css/rime/css-images/arrow-fr.gif"
									style="border:none;" title="Fast Backwards" />
							</f:facet>
						</ice:dataPaginator>

						<ice:outputText value="" />

						<!-- Display counts about the table and the currently displayed page -->
						<ice:dataPaginator id="dataScroll_2" for="idProductos"
							style="font-size: 9px;" rowsCountVar="rowsCount"
							displayedRowsCountVar="displayedRowsCount"
							firstRowIndexVar="firstRowIndex" lastRowIndexVar="lastRowIndex"
							pageCountVar="pageCount" pageIndexVar="pageIndex">
							<ice:outputFormat
								value="{0} registros encontrados, mostrando {1}, de {2} hasta {3}. Pagina {4} / {5}."
								styleClass="standard">
								<f:param value="#{rowsCount}" />
								<f:param value="#{displayedRowsCount}" />
								<f:param value="#{firstRowIndex}" />
								<f:param value="#{lastRowIndex}" />
								<f:param value="#{pageIndex}" />
								<f:param value="#{pageCount}" />
							</ice:outputFormat>
						</ice:dataPaginator>
					</ice:panelGrid>					
				</ice:panelGroup>
				<ice:panelGroup rendered="#{mBeanEntrantes.verNuevo}">
					<ice:panelGrid columns="1" style="font-size: 9px;" border="1">
					<ice:panelGrid columns="6" style="font-size: 9px;">
						<ice:outputText value="REGISTRO :" />
						<ice:inputText value="#{mBeanEntrantes.ncorrelativoN}"
							disabled="true" />
						<ice:outputText value="AÑO :" />
						<ice:inputText value="#{mBeanEntrantes.nanoN}" readonly="True"
							disabled="true" />
						<ice:outputText value="AREA ORIGEN :" />
						<ice:inputText value="#{mBeanEntrantes.area_origenN}"
							readonly="True" disabled="true" size="50" />
						<ice:outputText value="ORIGEN :" />
						<ice:selectOneMenu value="#{mBeanEntrantes.origenN}"
							style="font-size: 9px;"
							disabled="true"
							valueChangeListener="#{mBeanEntrantes.captura_origen}">
							<f:selectItems value="#{mBeanEntrantes.items8}"/>
						</ice:selectOneMenu>
						<ice:outputText value="TIPO DOCUMENTO :" />
						<ice:selectOneMenu value="#{mBeanEntrantes.vtipodocN}"
							disabled="true"
							style="font-size: 9px;" title="Tipos">
							<f:selectItems value="#{mBeanEntrantes.items2}" />
						</ice:selectOneMenu>
						<ice:outputText value="NUMERO DOC :" />
						<ice:inputText value="#{mBeanEntrantes.vnumdocN}"
							onchange="this.value = this.value.toUpperCase();" size="50" disabled="true"
							maxlength="35" />
						<ice:outputText value="FEC.DOC. :" />
						<ice:selectInputDate renderMonthAsDropdown="true"
							renderYearAsDropdown="true" value="#{mBeanEntrantes.dfecdocN}" 
							style="font-size: 9px;" title="Fec1" renderAsPopup="true">
							<f:convertDateTime pattern="dd/MM/yyyy"
								timeZone="#{dateSelect.timeZone}" />
						</ice:selectInputDate>
						<ice:outputText value="FECHA RECEP:" />
						<ice:selectInputDate renderMonthAsDropdown="true"
							renderYearAsDropdown="true"
							value="#{mBeanEntrantes.dfecregistroN}" style="font-size: 9px;" 
							title="Fec1" renderAsPopup="true">
							<f:convertDateTime pattern="dd/MM/yyyy"
								timeZone="#{dateSelect.timeZone}" />
						</ice:selectInputDate>
						<ice:outputText value="FEC.PLAZO :" />
						<ice:selectInputDate renderMonthAsDropdown="true" disabled="true"
							renderYearAsDropdown="true" value="#{mBeanEntrantes.dfecplazoN}" 
							style="font-size: 9px;" title="Fec1" renderAsPopup="true">
							<f:convertDateTime pattern="dd/MM/yyyy"
								timeZone="#{dateSelect.timeZone}" />
						</ice:selectInputDate>
						<ice:outputText value="PRIORIDAD :" />
						<h:selectOneMenu value="#{mBeanEntrantes.vprioridadN}"
							style="font-size: 9px;" disabled="true">
							<f:selectItem itemValue="URGENTE" itemLabel="URGENTE" />
							<f:selectItem itemValue="ALTA" itemLabel="ALTA" />
							<f:selectItem itemValue="MEDIA" itemLabel="MEDIA" />
							<f:selectItem itemValue="BAJA" itemLabel="BAJA" />
						</h:selectOneMenu>
						<ice:outputText value="ESTADO :" />
						<h:selectOneMenu value="#{mBeanEntrantes.vestadoN}"
							style="font-size: 9px;" disabled="true">
							<f:selectItem itemValue="ESTA001" itemLabel="PENDIENTE" />
							<f:selectItem itemValue="ESTA003" itemLabel="ATENDIDO" />
						</h:selectOneMenu>
						<ice:outputText value="DIAS :" />
						<ice:inputText value="#{mBeanEntrantes.ndiasplazoN}" size="10"
							maxlength="3" onkeypress="49" disabled="true" />
					</ice:panelGrid>
					</ice:panelGrid>
					<ice:panelGrid columns="5" style="font-size: 9px;">
					<ice:panelGrid columns="1" style="font-size: 9px;" border="1">
						<ice:outputText value="REMITENTE :" />
						<ice:selectInputText id="AutocomletaRemitente" rows="40" 
                                value="#{mBeanEntrantes.personaSeleccionada}" 
                                disabled="true"                               
                                valueChangeListener="#{mBeanEntrantes.autocompletarPersona}"                                
                                listVar="_persona"                                 
                                width="350px;font-size:10px;color:blue;"    
                                listValue="#{mBeanEntrantes.posiblespersonas}">
                 		<f:facet name="selectInputText">
                     	<ice:panelGrid columns="2">                         
                         <ice:outputText id="nombre"  style="font-size: 9px;" 
                         value="#{_persona.descripcion}" />
                     	</ice:panelGrid>
                		 </f:facet>
             			</ice:selectInputText>
						
						<ice:outputText value="REMITIDO POR :" />
						<ice:selectOneMenu value="#{mBeanEntrantes.ficha_dirigidoN}"
							style="font-size: 9px; color: blue" disabled="true">
							<f:selectItems value="#{mBeanEntrantes.items3}" />
						</ice:selectOneMenu>
						<ice:commandButton
						actionListener="#{mBeanEntrantes.eventLimpiar}"
						title="Limpiar" image="/images/wand.png"
						style="size: width: 25px; height: 25px" />						
						<!-- 
						<ice:selectOneMenu value="#{mBeanEntrantes.vremitenteN}"
							style="font-size: 9px;"
							valueChangeListener="#{mBeanEntrantes.captura}">
							<f:selectItems value="#{mBeanEntrantes.items9}" />
						</ice:selectOneMenu>
					<ice:commandButton
						actionListener="#{mBeanEntrantes.eventRefrescarCombo}"
						title="Refresh" image="/images/resert.png"
						style="size: width: 20px; height: 20px" />
					 -->
					</ice:panelGrid>
					<ice:panelGrid columns="4" style="font-size: 9px;">
						<ice:outputText value="NUEVO REMITENTE EXT.:" />
						<ice:inputText value="#{mBeanEntrantes.empresa}"
							onchange="this.value = this.value.toUpperCase();"							
							disabled="#{mBeanEntrantes.editarEmpresa}" size="80" maxlength="80"/>
						<ice:commandButton actionListener="#{mBeanEntrantes.verRemitente}"
							title="Agregar Empresa" 
							image="/images/mas.png"
							style="size: width: 20px; height: 20px" />
						<ice:commandButton
							actionListener="#{mBeanEntrantes.eventGrabarCombo}"
							disabled="#{mBeanEntrantes.disBotonGrabar}" title="Grabar"
							image="/images/save.png" style="size: width: 20px; height: 20px" />
					</ice:panelGrid>
					<ice:panelGrid columns="6" style="font-size: 9px;">					    
						<ice:outputText value="SELECCIONAR :" />
						<ice:selectOneMenu value="#{mBeanEntrantes.dirigidoN}"
							style="font-size: 9px;" title="Derivado" disabled="true">
							<f:selectItems value="#{mBeanEntrantes.items1}" />
						</ice:selectOneMenu>
						<ice:panelGrid columns="1" style="font-size: 9px;">
						<ice:outputText value="DESTINATARIO" />
						<ice:selectOneRadio value="#{mBeanEntrantes.estadoSeleccionado}" layout="pageDirection">					      
					       <f:selectItem itemValue="AA" itemLabel="TO" />
					       <f:selectItem itemValue="CC" itemLabel="CC" />
					    </ice:selectOneRadio>
						</ice:panelGrid>
						<ice:panelGrid columns="1" style="font-size: 9px;">							
							<ice:commandButton value="Añadir" title="Añadir"
								image="/images/plus.png" style="size: width: 30px; height: 30px"
								actionListener="#{mBeanEntrantes.pasarItems}" />
							<ice:commandButton value="Quitar" title="Quitar"
								image="/images/minus.png" style="size: width: 30px; height: 30px"
								actionListener="#{mBeanEntrantes.quitarItems}" />
						</ice:panelGrid>
						<ice:dataTable var="item"
							value="#{mBeanEntrantes.seleccionados}"  
							columnClasses="tableCol" style="font-size: 9px;" 
							disabled="true" id="idSelecc" scrollable="false">
							<ice:column>
							   <ice:rowSelector  value="#{item.selected}" 								
								preStyleOnSelection="true" />
								<f:facet name="header">
