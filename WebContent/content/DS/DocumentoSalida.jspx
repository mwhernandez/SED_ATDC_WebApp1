									<ice:outputText value="#{myDocs.ncorrelativo}"
										style="font-size: 9px;" />
								</ice:column>
								<ice:column>
									<f:facet name="header">
										<ice:outputText value="Num. Doc" />
									</f:facet>
									<ice:outputText value="#{myDocs.vnumdoc}"
										style="font-size: 9px;" />
								</ice:column>
								<ice:column>
									<f:facet name="header">
										<ice:outputText value="Asunto" />
									</f:facet>
									<ice:outputText value="#{myDocs.vasunto}"
										style="font-size: 9px;" />
								</ice:column>

							</ice:dataTable>
						</ice:panelGrid>
						<ice:panelGrid columns="2">
							<ice:commandButton type="submit" value="Aceptar"
								actionListener="#{mBeandocumentosalida.cerrarDocs}" />
							<ice:commandButton type="submit" value="Cancelar"
								actionListener="#{mBeandocumentosalida.cancelarDocs}" />
						</ice:panelGrid>
					</ice:panelGroup>
				</f:facet>
			</ice:panelPopup>
			-->

			<!-- Popup para cargar un archivo adjunto" />-->
			<ice:panelPopup draggable="false" modal="true"
				style="width: 550px; height: 350px; color: white; background-color: #0085BC;"
				visible="#{mBeandocumentosalida.verDetalles}" autoCentre="true"
				styleClass="icePnlPop">
				<f:facet name="header">
					<ice:panelGroup styleClass="popupHeaderWrapper">
						<ice:outputText value="Archivo Adjunto"
							styleClass="popupHeaderText" style="font-size: 12px; color: blue;"/>
					</ice:panelGroup>
				</f:facet>
				<f:facet name="body">
					<ice:panelGroup styleClass="popupBody">
						<ice:panelGrid columns="2">
							<ice:panelTab label="Carga de Datos" title="Carga de Datos">
								<ice:panelGroup>
									<!-- file upload usage with progress callback. -->
									<ice:panelGrid>
										<ice:outputText value="Seleccione un archivo a cargar:"
											style="font-size: 12px; color: white;" />
										<ice:inputFile id="inputFileName2" uniqueFolder="false"
											uploadDirectoryAbsolute="true" autoUpload="true"
											uploadDirectory="#{msg['path.salida']}"
											progressListener="#{mCarga.fileUploadProgress}"
											actionListener="#{mCarga.uploadFile}" />
										<!-- progress bar, percent value is upated via progressListener-->

										<ice:outputProgress value="#{mCarga.fileProgress}"
											styleClass="uploadProgressBar" />
										<ice:message for="inputFileName" />
										<br />
										<ice:outputText value="#{mCarga.componentStatus}"
											style="font-size: 12px; color: white;" />
									</ice:panelGrid>
								</ice:panelGroup>
							</ice:panelTab>

						</ice:panelGrid>
						<ice:commandButton id="modalPnlCBError" type="submit"
							value="Salir" image="/images/cerrar.png"
							style="size: width: 35px; height: 35px"
							actionListener="#{mBeandocumentosalida.cerrarDetalles}" />
					</ice:panelGroup>
				</f:facet>
			</ice:panelPopup>

			<!-- Popup para la carga -->
			<ice:panelPopup id="modalPnlPopCarga" draggable="false" modal="true"
				style="width: 200px; height: 120px; background-color: #0085BC;"
				visible="#{mCarga.verConfirmacion}" autoCentre="true"
				styleClass="icePnlPop">
				<f:facet name="header">
					<ice:panelGroup styleClass="popupHeaderWrapper">
						<ice:outputText value="Alerta" styleClass="popupHeaderText" />
					</ice:panelGroup>
				</f:facet>
				<f:facet name="body">
					<ice:panelGroup styleClass="popupBody">
						<ice:outputText value="#{mCarga.msg}" style="font-size: 12px; color: white;" />
						<br></br>
						<br></br>
						<ice:commandButton type="submit" value="Aceptar" title="Aceptar"
							image="/images/actualizar.png"
							style="size: width: 25px; height: 20px"
							actionListener="#{mCarga.okConfirm}" />
						<ice:outputText value="   " />
						<ice:commandButton value="Cancelar" title="Cancelar"
							image="/images/eliminar2.png"
							style="size: width: 25px; height: 20px"
							actionListener="#{mCarga.cancelar}" />
					</ice:panelGroup>
				</f:facet>
			</ice:panelPopup>

			<!-- Popup para mensaje -->
			<ice:panelPopup draggable="false" modal="true"
				style="width: 200px; height: 120px; background-color: #0085BC;" visible="#{mCarga.verAlerta}"
				autoCentre="true" styleClass="icePnlPop">
				<f:facet name="header">
					<ice:panelGroup styleClass="popupHeaderWrapper">
						<ice:outputText value="Alerta" styleClass="popupHeaderText"
						style="font-size: 12px; color: white;" />
					</ice:panelGroup>
				</f:facet>
				<f:facet name="body">
					<ice:panelGroup styleClass="popupBody">
						<ice:outputText value="#{mCarga.msgAlerta}" style="font-size: 12px; color: white;" />
						<br></br>
						<br></br>
						<ice:commandButton type="submit" value="Aceptar"
							image="/images/actualizar.png"
							style="size: width: 20px; height: 20px"
							actionListener="#{mCarga.okAlerta}" />

					</ice:panelGroup>
				</f:facet>
			</ice:panelPopup>
			
			<!-- Popup para aviso de Visado -->
			<ice:panelPopup id="modalPnlPopVisado" draggable="false" modal="true"
				style="width: 300px; height: 140px; background-color: #0085BC;"
				visible="#{mBeandocumentosalida.verSignet}" autoCentre="true"
				styleClass="icePnlPop icePnlPopVisa">
				<f:facet name="header">
					<ice:panelGroup styleClass="popupHeaderWrapper">
						<ice:outputText value="Alerta" styleClass="popupHeaderText" />
					</ice:panelGroup>
				</f:facet>
				<f:facet name="body">
					<ice:panelGroup styleClass="popupBody">
						<ice:outputText value="#{mBeandocumentosalida.msgVisado}" style="font-size: 12px; color: white;" />
						<br></br>
						<br></br>
							<ice:commandButton styleClass="callSignetButton" value="Aceptar" title="Aceptar"
								image="/images/actualizar.png"
								style="width: 20px; height: 20px" onclick="callSignet();" actionListener="#{mBeandocumentosalida.callSignet}"></ice:commandButton>
							
							<ice:commandButton value="Cancelar" title="Cancelar"
							image="/images/eliminar2.png"
							style="width: 20px; height: 20px"
							actionListener="#{mBeandocumentosalida.eventCancelar}" />
							
						<div id="firma-container">
							<form id="loginForm" method="post" action="#{mBeandocumentosalida.signetUrl}" target="iframeFirma">
								<input type="hidden" name="listarArchivos" value="1"/>
								<input type="hidden" name="activarDescripcion" value="0"/>
								<input type="hidden" name="usarPersonalizado" value="1"/>
								<input type="hidden" name="firmarPfxAlmacen" value="1"/>
								<input type="hidden" name="aplicarImagen" value="1"/>
								<input type="hidden" name="nombreArchivos" value="#{mBeandocumentosalida.signetFile}"/>
								<input type="hidden" name="rutaOrigen" value="#{mBeandocumentosalida.signetPathOrigin}"/>
								<input type="hidden" name="rutaDestino" value="#{mBeandocumentosalida.signetPathDestination}"/>
								<input type="hidden" name="tipoFirma" value="1"/>
								<input type="hidden" name="urlConfigService" value="#{mBeandocumentosalida.signetConfig}"/>
								<input type="hidden" name="webService" value="#{mBeandocumentosalida.signetWs}"/>
								<input type="hidden" name="nomarch" value=""/>
								<input type="hidden" name="cargo" value=""/>
								<input type="hidden" name="razon" value=""/>
								<input type="hidden" name="ubicacion" value=""/>
								<input type="hidden" name="invisible" value="1"/>
								<input type="hidden" name="posicionFirma" value="#{mBeandocumentosalida.verVisar ? mBeandocumentosalida.signetPosicionVisado : mBeandocumentosalida.signetPosicionFirmado}"/>
							</form>
							<iframe frameborder="0" name="iframeFirma" id="iframeFirma" width="250" height="140"/>
						</div>
						
					</ice:panelGroup>
				</f:facet>
			</ice:panelPopup>

		</ice:panelTab>
	</ice:panelTabSet>



	<ice:menuPopup id="menuPopupEffects">
		<ice:menuItem id="mnuModificar" value="Modificar"
			title="Modificar perfil">
		</ice:menuItem>

		<ice:menuItem id="mnuAuditoria" value="Auditoria"
			title="Consultar auditoria">
		</ice:menuItem>


	</ice:menuPopup>


</ice:form>

<ice:form style="display: none;">
	<input id="signetMessage" type="hidden" name="signetMessage" value="-1" />
	<h:commandButton styleClass="signetMessageButton" style="display: none;" actionListener="#{mBeandocumentosalida.signetMensaje}" type="submit" />
</ice:form>

</body>
</html>
