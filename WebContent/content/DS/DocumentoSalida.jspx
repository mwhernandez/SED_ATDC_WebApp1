<body>
<script type="text/javascript">
	(function() {
		window.addEventListener("message", function (e) {
			setSignetMessage(e.data);
			hideSignet();
			window.close();
		});
	})();
</script>

<ice:form partialSubmit="true">
	<ice:loadBundle basename="com.sedapal.tramite.files.parametros"
		var="msg" />
	<ice:panelTabSet id="icePnlTbSet1">

		<!-- Panel Tab 1 defined with some basic content 
		scrollable="true" scrollHeight="500px"
		columnClasses="tableCol" scrollable="true" scrollHeight="250px"
		-->
		<ice:panelTab label="Documentos Salientes" title="Catalogo">
			<ice:panelGroup rendered="#{mBeandocumentosalida.verCatalogo}">
				<ice:panelGrid columns="#{!mBeandocumentosalida.disverfirma || !mBeandocumentosalida.disgenerar_firma  ? '6': '5'}" cellpadding="#{!mBeandocumentosalida.disverfirma || !mBeandocumentosalida.disgenerar_firma ? '6': '5'}">
					<!-- disabled="#{mBeandocumentosalida.verEliminar}" -->
					<ice:commandButton value="Nuevo"
						actionListener="#{mBeandocumentosalida.eventNuevo}" title="Nuevo"
						image="/images/nuevo2.png" style="size: width: 40px; height: 40px" />
					<ice:commandButton value="Eliminar"	
						actionListener="#{mBeandocumentosalida.eventEliminar}"
						title="Eliminar" image="/images/eliminar2.png"
						style="size: width: 40px; height: 40px" />
					<ice:commandButton value="Actualizar"
						actionListener="#{mBeandocumentosalida.eventActualizar}"
						title="Modificar" image="/images/modificar2.png"
						style="size: width: 40px; height: 40px" />
															
					<ice:dataExporter for="idProductos" ignorePagination="true"
						label="Excel" type="excel" image="/images/excel2010.jpg"
						style="border:0;"
						popupBlockerLabel="Popup Blocker detected - click here to resume download..."
						clickToCreateFileText="Excel" />
					<ice:commandButton value="Refrescar"
						actionListener="#{mBeandocumentosalida.eventRefrescar}"
						title="Refrescar" image="/images/resert.png"
						style="size: width: 25px; height: 25px">
					</ice:commandButton>
					<c:if test="#{!mBeandocumentosalida.disverfirma}">
				        <ice:commandButton value="Firmar"
						actionListener="#{mBeandocumentosalida.eventFirmar}"
						title="Firmar" image="/images/firmar.png"
						style="width: 30px; height: 30px" />
				    </c:if>
				    <c:if test="#{!mBeandocumentosalida.disgenerar_firma}">
				        <ice:commandButton value="Visar"
						actionListener="#{mBeandocumentosalida.eventVisar}"
						title="Visar" image="/images/firmar.png"
						style="width: 30px; height: 30px" />
				    </c:if>
				
					<ice:outputText value="Nuevo" style="font-size: 10px; color: blue;"/>
				
					<ice:outputText value="Eliminar" style="font-size: 10px; color: blue;"/>					
									
					<ice:outputText value="Actualizar" style="font-size: 10px; color: blue;"/>					
														
					<ice:outputText value="Excel" style="font-size: 10px; color: blue;"/>					
									
					<ice:outputText value="Refrescar"  style="font-size: 9px; color: blue;"/>
					
					<c:if test="#{!mBeandocumentosalida.disverfirma}">
						<ice:outputText value="Firmar"  style="font-size: 9px; color: blue;"/>
					</c:if>
					
					<c:if test="#{!mBeandocumentosalida.disgenerar_firma}">
						<ice:outputText value="Visar"  style="font-size: 9px; color: blue;"/>
					</c:if>
					
				</ice:panelGrid>
				
				
				
				<ice:panelGrid columns="3" border="1">
				<ice:panelGrid columns="11">
					<ice:outputLabel value="F.INICIAL:" style="font-size: 9px; color: green;"></ice:outputLabel>
					<ice:selectInputDate style="font-size: 9px"
						renderMonthAsDropdown="true" renderYearAsDropdown="true"
						value="#{mBeandocumentosalida.date1}" title="Fec1"
						renderAsPopup="true">
						<f:convertDateTime pattern="dd/MM/yyyy"
							timeZone="#{dateSelect.timeZone}" />
					</ice:selectInputDate>
					<ice:outputLabel value="F.FINAL:" style="font-size: 9px; color: green;"></ice:outputLabel>
					<ice:selectInputDate style="font-size: 9px"
						renderMonthAsDropdown="true" renderYearAsDropdown="true"
						value="#{mBeandocumentosalida.date2}" title="Fec2"
						renderAsPopup="true">
						<f:convertDateTime pattern="dd/MM/yyyy"
							timeZone="#{dateSelect.timeZone}" />
					</ice:selectInputDate>
					 <ice:selectOneMenu value="#{mBeandocumentosalida.tipoopcion}"
                                        valueChangeListener="#{mBeandocumentosalida.eventtipoconsulta}"
                                        style="font-size: 9px;"
                                        title="Tipo de Consulta">
                    <f:selectItems value="#{mBeandocumentosalida.itemscombo}" />
                    </ice:selectOneMenu>
                    <ice:outputLabel value="TIPO DOC:" style="font-size: 9px; color: green;"></ice:outputLabel>
					<ice:selectOneMenu binding="#{mBeandocumentosalida.combo}"
						disabled="#{mBeandocumentosalida.disverestado}"
						style="font-size: 9px"
						value="#{mBeandocumentosalida.itemSeleccionado}" 
						title="Tipo Doc">
						<f:selectItems id="slctCreacionUsuario"
							value="#{mBeandocumentosalida.items2}" />
					</ice:selectOneMenu> 
					<!-- <ice:outputLabel value="ESTADO:" style="font-size: 9px; color: green;"></ice:outputLabel>
					<ice:selectOneMenu binding="#{mBeandocumentosalida.comboEstado}"
						disabled="#{mBeandocumentosalida.disverestado_firma}"
						style="font-size: 9px"
						value="#{mBeandocumentosalida.itemEstadoSeleccionado}" 
						title="Estado">
						<f:selectItems id="slctCreacionUsuario"
							value="#{mBeandocumentosalida.itemsEstado}" />
					</ice:selectOneMenu>  -->
					<ice:outputLabel value="REMITENTE:" 
					style="font-size: 9px; color: green;"></ice:outputLabel>
					<ice:selectOneMenu binding="#{mBeandocumentosalida.comboarea}"
						disabled="#{mBeandocumentosalida.disverremite}"
						style="font-size: 10px; font-weight: bold;" 
						value="#{mBeandocumentosalida.itemareaSeleccionado}"
						title="Remitente">
						<f:selectItems id="slctCreacionUsuario"
							value="#{mBeandocumentosalida.items11}" />
					</ice:selectOneMenu>
					</ice:panelGrid>
					
					
					<ice:commandButton value="Ejecutar"
						actionListener="#{mBeandocumentosalida.eventFiltros}"
						image="/images/buscar2.png"
						style="size: width: 25px; height: 25px">
					</ice:commandButton>					
					<ice:outputResource id="pdf-image" shared="false" 
					    label="Impresión"
					    rendered="#{mBeandocumentosalida.verReport1}"
						attachment="false" fileName="reportedespacho.pdf"
						image="/images/impresion.png"
						style="border=0 size: width: 18px; height: 18px"
						mimeType="application/pdf"
						resource="#{mBeandocumentosalida.recursoReport}">
					</ice:outputResource>
					
					<ice:outputResource id="pdf-image22" shared="false"
						label="Impresión"
						rendered="#{mBeandocumentosalida.verReport2}"
					    attachment="false" fileName="reportedespachoremite.pdf"
						image="/images/impresion.png"
						style="border=0 size: width: 18px; height: 18px"
						mimeType="application/pdf"
						resource="#{mBeandocumentosalida.recursoReporteNuevo}" />
											 				
				</ice:panelGrid>
				<ice:panelGrid columns="7" cellpadding="1" border="1">
					<h:selectOneRadio value="#{mBeandocumentosalida.opcion}"
						style="font-size: 9px; color: green;">
						<f:selectItem itemValue="CODIGO" itemLabel="REGISTRO" />
						<f:selectItem itemValue="NUMDOC" itemLabel="NUM.DOC" />
						<f:selectItem itemValue="DIRIGIDO" itemLabel="DIRIGIDO" />
						<f:selectItem itemValue="ASUNTO" itemLabel="ASUNTO" />
						<f:selectItem itemValue="TIPO" itemLabel="TIPO DOC." />
						<f:selectItem itemValue="REFERENCIA" itemLabel="REFERENCIA" />
						<f:selectItem itemValue="ENTRADA" itemLabel="REG.ENTRADA" />
						<f:selectItem itemValue="USUARIO" itemLabel="USUARIO" />
					</h:selectOneRadio>
					<ice:inputText value="#{mBeandocumentosalida.detalle}"
						onchange="this.value = this.value.toUpperCase();"
						style="font-size: 9px;" size="70" />
					<ice:commandButton value="Ejecutar"
						actionListener="#{mBeandocumentosalida.eventBusquedas}"
						title="Buscar" image="/images/buscar2.png"
						style="size: width: 25px; height: 25px">
					</ice:commandButton>
				</ice:panelGrid>


				<!---->
			</ice:panelGroup>
			
			<ice:panelGroup rendered="#{mBeandocumentosalida.verCatalogo}">
				<ice:dataTable var="pro" value="#{mBeandocumentosalida.documento}"
					columnClasses="tableCol" style="font-size: 9px;" rows="10"
					bgcolor="#81BEF7"
					sortColumn="#{mBeandocumentosalida.sortColumnName}"
					sortAscending="#{mBeandocumentosalida.ascending}" id="idProductos"
					scrollable="false">
					<ice:column>
						<ice:rowSelector id="selected" value="#{pro.selected}"
							multiple="#{mBeandocumentosalida.multiple}"
							enhancedMultiple="#{mBeandocumentosalida.enhancedMultiple}"
							selectionListener="#{mBeandocumentosalida.rowSelectionListener}"
							preStyleOnSelection="true" />
						<f:facet name="header">
							<ice:outputText value="Reg"/>
							<ice:commandSortHeader columnName="codigo" arrow="true">
								<ice:outputLabel value="Reg" style="color: blue;"/>
							</ice:commandSortHeader>
						</f:facet>
						<ice:outputText id="idCB" value="#{pro.codigo}" />
					</ice:column>
					<!-- 
					<ice:column>
					<f:facet name="header">					
							<ice:outputText value="Reg" />														
						</f:facet>
						<ice:outputText id="idCB" value="#{pro.codigo}" />
					</ice:column>
					 -->
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Num.Doc" style="color: blue;"/>
							<!-- 	
							<ice:commandSortHeader columnName="numerodoc" arrow="true">
									<ice:outputLabel value="Num.Doc" />
							</ice:commandSortHeader>
							 -->
						</f:facet>
						<ice:outputText value="#{pro.numerodoc}" />
					</ice:column>
					<!-- 					
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Remitente" />
						</f:facet>
						<ice:outputText value="#{pro.nom_area}" />
					</ice:column>
					 -->
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Dirigido" />
							<ice:commandSortHeader columnName="dirigido" arrow="true">
								<ice:outputLabel value="Dirigido" style="color: blue;"/>
							</ice:commandSortHeader>
						</f:facet>
						<ice:outputText value="#{pro.dirigido}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Asunto" style="color: blue;"/>
							<!--
							<ice:commandSortHeader columnName="asunto" arrow="true">
									<ice:outputLabel value="Asunto" />
							</ice:commandSortHeader>
							 -->
						</f:facet>
						<ice:outputText value="#{pro.asunto}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Tipo Doc" />
							<ice:commandSortHeader columnName="tipodoc" arrow="true">
								<ice:outputLabel value="Tipo Doc" style="color: blue;"/>
							</ice:commandSortHeader>
						</f:facet>
						<ice:outputText value="#{pro.tipodoc}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Prio." style="color: blue;"/>
						</f:facet>
						<ice:outputText value="#{pro.prioridad}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Fecha Doc." />
							<ice:commandSortHeader columnName="fecha" arrow="true">
								<ice:outputLabel value="Fecha Doc." style="color: blue;"/>
							</ice:commandSortHeader>
						</f:facet>
						<ice:outputText value="#{pro.fecha}">
							<f:convertDateTime pattern="dd/MM/yyyy" />
						</ice:outputText>
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Dias" style="color: blue;"/>
						</f:facet>
						<ice:outputText value="#{pro.dias}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Fecha Plazo" />
							<ice:commandSortHeader columnName="dfecplazo" arrow="true">
								<ice:outputLabel value="Fecha Plazo" style="color: blue;"/>
							</ice:commandSortHeader>
						</f:facet>
						<ice:outputText value="#{pro.dfecplazo}">
							<f:convertDateTime pattern="dd/MM/yyyy" />
						</ice:outputText>
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Reg.Entrada" style="color: blue;"/>
						</f:facet>
						<ice:outputText value="#{pro.valor_entrada}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Usuario" style="color: blue;"/>							
						</f:facet>
						<ice:outputText value="#{pro.vrescre}" />
					</ice:column>
					<ice:column>
							<f:facet name="header">
								<ice:outputText value="Estado" style="color: blue;"/>
							</f:facet>
							<ice:outputText value="#{pro.estado}" />
					</ice:column>
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Est" />
							<ice:commandSortHeader columnName="estado" arrow="true">
								<ice:outputLabel value="Est" style="color: blue;"/>
							</ice:commandSortHeader>
						</f:facet>
						<h:graphicImage id="gi" alt="#{pro.estado}"
							value="images/#{pro.estado}.png"
							style="size: width: 15px; height: 15px">
						</h:graphicImage>
					</ice:column>
					<!--  
					<ice:column>
							<f:facet name="header">
								<ice:outputText value="Indicador" style="color: blue;"/>
							</f:facet>
							<ice:outputText value="#{pro.indicador}" />
					</ice:column>
					-->
					<ice:column>
						<f:facet name="header">
							<ice:outputText value="Adj." style="color: blue;"/>
						</f:facet>
						<!--ubiarchivo  EDH 03.07.2023 -->
						
						<ice:outputLink value="#{pro.ubicacion_salida}" target="_blank"
							rendered="#{pro.atachment}">
							<ice:graphicImage id="gi1" value="images/#{pro.indicador}.jpg"
								style="border:0; size: width: 25px; height: 25px">
							</ice:graphicImage>
						</ice:outputLink>
						
						 <!--
						<ice:commandButton value="VisualizarPDF"
								actionListener="#{mBeandocumentosalida.evenVerPDF}"
								title="Visualizar PDF" image="images/#{pro.indicador}.jpg"
								style="size: width: 25px; height: 25px" />
						-->
						
					</ice:column>
					
				</ice:dataTable>


				<ice:panelGrid columns="2">
					<ice:outputText value=" " />
					<!-- Paginator with page controls -->
					<ice:dataPaginator id="dataScroll_3" for="idProductos"
						paginator="true" fastStep="3" paginatorMaxPages="5">
						<f:facet name="first">
							<ice:graphicImage
								url="./xmlhttp/css/rime/css-images/arrow-first.gif"
								style="border:none;" title="First Page" />
						</f:facet>
						<f:facet name="last">
							<ice:graphicImage
								url="./xmlhttp/css/rime/css-images/arrow-last.gif"
								style="border:none;" title="Last Page" />
						</f:facet>
						<f:facet name="previous">
							<ice:graphicImage
								url="./xmlhttp/css/rime/css-images/arrow-previous.gif"
								style="border:none;" title="Previous Page" />
						</f:facet>
						<f:facet name="next">
							<ice:graphicImage
								url="./xmlhttp/css/rime/css-images/arrow-next.gif"
								style="border:none;" title="Next Page" />
						</f:facet>
						<f:facet name="fastforward">
							<ice:graphicImage
								url="./xmlhttp/css/rime/css-images/arrow-ff.gif"
								style="border:none;" title="Fast Forward" />
						</f:facet>
						<f:facet name="fastrewind">
							<ice:graphicImage
								url="./xmlhttp/css/rime/css-images/arrow-fr.gif"
								style="border:none;" title="Fast Backwards" />
						</f:facet>
					</ice:dataPaginator>

					<ice:outputText value="" />

					<!-- Display counts about the table and the currently displayed page -->
					<ice:dataPaginator id="dataScroll_2" for="idProductos"
						rowsCountVar="rowsCount"
						displayedRowsCountVar="displayedRowsCount"
						firstRowIndexVar="firstRowIndex" lastRowIndexVar="lastRowIndex"
						pageCountVar="pageCount" pageIndexVar="pageIndex">
						<ice:outputFormat
							value="{0} documentos encontrados, mostrando {1}, de {2} hasta {3}. Pagina {4} / {5}."
							styleClass="standard">
							<f:param value="#{rowsCount}" />
							<f:param value="#{displayedRowsCount}" />
							<f:param value="#{firstRowIndex}" />
							<f:param value="#{lastRowIndex}" />
							<f:param value="#{pageIndex}" />
							<f:param value="#{pageCount}" />
						</ice:outputFormat>
					</ice:dataPaginator>
				</ice:panelGrid>
				<!-- 
				<ice:panelGrid columns="1">
								<ice:dataExporter for="idProductos" ignorePagination="true"
									rendered="#{mBeandocumentosalida.disPrint }"
									label="Click para bajar el reporte en excel" type="excel"
									popupBlockerLabel="Popup Blocker detected - click here to resume download..."
									style="border:none; size: width: 18px; height: 18px;"									
									clickToCreateFileText="Click para crear archivo en excel.." />
					</ice:panelGrid>
				 -->
			</ice:panelGroup>

			<ice:panelGroup rendered="#{mBeandocumentosalida.verNuevo}">
				<ice:panelGrid border="1">
				<ice:panelGrid columns="6" style="font-size: 9px;">
					<ice:outputText value="REGISTRO :" />
					<ice:inputText value="#{mBeandocumentosalida.codigoN}"
						readonly="True" disabled="true" />
					<ice:outputText value="AÑO :" />
					<ice:inputText value="#{mBeandocumentosalida.anoN}" readonly="True"
						disabled="true" />
					<!-- 					
					<h:selectOneMenu value="#{mBeandocumentosalida.origenesN}" 
						style="font-size: 9px;"
						valueChangeListener="#{mBeandocumentosalida.captura_origen}">
						<f:selectItem itemValue="SELECCIONAR" itemLabel="SELECCIONAR" />
						<f:selectItem itemValue="INTERNO" itemLabel="INTERNO" />
						<f:selectItem itemValue="EXTERNO" itemLabel="EXTERNO" />
					</h:selectOneMenu>
					 -->
					<ice:outputText value="PRIORIDAD :" />
					<h:selectOneMenu value="#{mBeandocumentosalida.prioridadN}"
						style="font-size: 9px;">
						<f:selectItem itemValue="URGENTE" itemLabel="URGENTE" />
						<f:selectItem itemValue="ALTA" itemLabel="ALTA" />
						<f:selectItem itemValue="MEDIA" itemLabel="MEDIA" />
						<f:selectItem itemValue="BAJA" itemLabel="BAJA" />
					</h:selectOneMenu>
					<ice:outputText value="NRO DOC. :" />
					<ice:inputText value="#{mBeandocumentosalida.numerodocN}" size="35"
						readonly="True" disabled="true" />
					<ice:outputText value="FECHA" />
					<ice:inputText value="#{mBeandocumentosalida.fechaN}"
						readonly="True" disabled="true">
						<f:convertDateTime pattern="dd/MM/yyyy" />
					</ice:inputText>
					<ice:outputText value="AREA REMITE :" />
					<ice:inputText value="#{mBeandocumentosalida.nom_areaN}"
						readonly="True" disabled="true" size="60" />
					<ice:outputText value="FECHA DOC:" />
					<ice:selectInputDate renderMonthAsDropdown="true"
						renderYearAsDropdown="true" value="#{mBeandocumentosalida.fechaN}"
						style="font-size: 9px;" title="Fec1" renderAsPopup="true">
						<f:convertDateTime pattern="dd/MM/yyyy"
							timeZone="#{dateSelect.timeZone}" />
					</ice:selectInputDate>
					<ice:outputText value="FECHA PLAZO :" />
					<ice:selectInputDate renderMonthAsDropdown="true"
						renderYearAsDropdown="true"
						value="#{mBeandocumentosalida.dfecplazoN}" style="font-size: 9px;"
						title="Fec1" renderAsPopup="true">
						<f:convertDateTime pattern="dd/MM/yyyy"
							timeZone="#{dateSelect.timeZone}" />
					</ice:selectInputDate>
					<ice:outputText value="DIAS :" />
					<ice:inputText value="#{mBeandocumentosalida.diasN}" size="10" />
					<ice:outputText value="ESTADO :" />
					<ice:selectOneMenu value="#{mBeandocumentosalida.nestadoN}"
						valueChangeListener="#{mBeandocumentosalida.capturaPerfil}"
						style="font-size: 9px;" title="Derivado">
						<f:selectItems value="#{mBeandocumentosalida.items9}" />
					</ice:selectOneMenu>					
					<ice:outputText value="ORIGEN :" />
					<ice:selectOneMenu value="#{mBeandocumentosalida.origenesN}"
						style="font-size: 9px;" title="Origen"
						valueChangeListener="#{mBeandocumentosalida.captura_origen}">
						<f:selectItems value="#{mBeandocumentosalida.items7}" />
					</ice:selectOneMenu>										
					<ice:outputText value="TIPO :" />
					<ice:selectOneMenu value="#{mBeandocumentosalida.tipodocN}"
						style="font-size: 9px;" title="Tipos">
						<f:selectItems value="#{mBeandocumentosalida.items2}" />
					</ice:selectOneMenu>
				</ice:panelGrid>
				</ice:panelGrid>
				<ice:panelGrid border="1">
				<ice:panelGrid columns="2" style="font-size: 9px;">
					<ice:outputText value="REMITENTE JEFE EQUIPO:" />
					<ice:selectOneMenu value="#{mBeandocumentosalida.remitenteN}"
						style="font-size: 11px;" title="Remite" disabled="true">
						<f:selectItems value="#{mBeandocumentosalida.items6}" />
					</ice:selectOneMenu>
